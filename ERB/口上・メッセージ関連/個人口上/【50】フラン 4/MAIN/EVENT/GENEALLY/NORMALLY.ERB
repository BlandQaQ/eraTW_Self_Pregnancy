;贤者之血調教中
@k1011BloodTrain
	;恋慕
	IF k1011GetTalent("恋慕")
		CALL k1011GPC("W", @"「啊……」", TARGET)
		CALL k1011GPC("W", @"「要、要、要、要! !要去了! !♪」", TARGET)
		CALL k1011GPC("W", @"「%k1011Sound("Happy", "啊、啊-啊! !")%去了、突然好厉害啊! !♪」", TARGET)
		CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%！快点玩弄%k1011Name(TARGET, TARGET)%!」", TARGET)
	;除此之外
	ELSE
		CALL k1011GPC("L", @"「%k1011Sound("Happy", "啊呀、啊呀! !")%舒服得不行了哦~ ~ ~ ~ ! !」", TARGET)
		CALL k1011GPC("W", @"「还想更猛烈些、快点想办法啊! ! !♪」", TARGET)
	ENDIF
	K1011BloodMedicine = 1

;贤者之血調教结束
@k1011BloodTrainEnd
	;恋慕
	IF k1011GetTalent("恋慕")
		CALL k1011GPC("W", @"「%k1011Sound("Happy", "哇、哇! !哇! !♪")%」", TARGET)
		CALL k1011GPC("W", @"「呀!%k1011Name(TARGET, MASTER)%！为什麼不继续做啊!」", TARGET)
		CALL k1011GPC("W", @"「这种事、芙兰一个人受不了哦~%k1011Name(TARGET, MASTER)%！！！」", TARGET)
		;除此之外
	ELSE
		CALL k1011GPC("L", @"「%k1011Sound("Happy", "啊、啊、")%有什麼来了、要去了…!」", TARGET)
		CALL k1011GPC("W", @"「%k1011Sound("Happy", "啊啊! !")%根本停不下来啊! !」", TARGET)
	ENDIF

