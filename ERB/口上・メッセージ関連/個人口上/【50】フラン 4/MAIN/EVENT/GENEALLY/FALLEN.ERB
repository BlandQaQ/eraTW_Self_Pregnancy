@k1011FallenLove
	DRAWLINE
	IF K1011Character == 1
		CALL k1011GPC("W", @"「.........」", TARGET)
		CALL k1011GP("W", @"芙兰用热切的视线注视着%CALLNAME:MASTER%")
		CALL k1011GP("W", @"而当%CALLNAME:MASTER%反过来注释芙兰时、她又慌慌张张地躲到床上、啪嗒啪嗒地晃动着小翅膀")
		CALL k1011GP("W", @"(看到%CALLNAME:MASTER%、心里好难受啊……)")
	ELSE
		CALL k1011GPC("W", @"「.........」", TARGET)
		CALL k1011GP("W", @"芙兰用热切的视线注视着%CALLNAME:MASTER%")
		CALL k1011GP("W", @"而当%CALLNAME:MASTER%反过来注释芙兰时、芙兰的脸渐渐红了")
