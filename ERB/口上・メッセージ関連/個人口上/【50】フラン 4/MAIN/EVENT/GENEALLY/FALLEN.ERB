;取得扶她通用判定早晚泄
@k1011GetFutaNormally
	SIF K1011Game == "TW"
		RETURN
	CALL k1011GSetQ("「她超持久的」", "「嗯、当然」", "「扶她当然是早泄的小鸡鸡啊!」")
	CALL k1011GInput()
	IF RESULT == 0
		CALL k1011GP("W", @"芙兰是一个特别持久、很了不起")
		CALL k1011SetTalent("持久", 1)
	ELSEIF RESULT == 1
		CALL k1011GP("W", @"芙兰和%CALLNAME:MASTER%一样、很了不起")
		CALL k1011SetTalent("持久", 0)
	ELSEIF RESULT == 2
		CALL k1011GP("W", @"芙兰、你要能再忍耐一下")
		CALL k1011SetTalent("持久", -1)
	ENDIF
