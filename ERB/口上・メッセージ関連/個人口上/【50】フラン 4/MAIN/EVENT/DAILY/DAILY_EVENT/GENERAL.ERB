		CALL k1011GPC("W", @"「现在%k1011Name(TARGET, TARGET)%最喜欢和%k1011Name(TARGET, MASTER)%玩和性有関的游戏」", TARGET)
	CALL k1011GPC("W", @"「不过%k1011Name(TARGET, MASTER)%想玩的话也是没问题的……禁忌『红莓陷阱』」", TARGET)
	CALL k1011GSleep()
	CALL k1011GPC("L", @"「啊、没事吧、%k1011Name(TARGET, MASTER)%」", TARGET)
	CALL k1011GPC("L", @"「芙兰没想到%k1011Name(TARGET, MASTER)%的弹幕水平这麼差、没留住手」", TARGET)
	CALL k1011AddBase("体力", - MIN(RAND:2500, k1011GetBase("体力", MASTER) - 1), MASTER)
	IF k1011GetBase("体力", MASTER) > 500
		IF k1011GetTalent("淫乱")
			LOCAL = RAND:((k1011GetBase("体力") + k1011GetBase("体力", MASTER)) / 500) + 2
			CALL k1011GP("L", @"看到%CALLNAME:MASTER%没事后、芙兰突然露出了一抹坏笑")
			CALL k1011GPC("L", @"「按照规矩、失败者是要被胜利者榨精的……♪」", TARGET)
			CALL k1011GP("W", @"芙兰熟练的解開了%CALLNAME:MASTER%的裤裆開始了骑乘位")
			CALL k1011GP("W", @"做了{LOCAL}次后芙兰才心满意足的放開%CALLNAME:MASTER%")
			CALL k1011AddJuel("欲情", LOCAL * 1000)
			CALL k1011AddExp("精液经验", LOCAL)
		ENDIF
	ELSE
		CALL k1011GP("W", @"芙兰慌忙把仅中了一发弹幕就倒地不起的%CALLNAME:MASTER%送回房间")
	ENDIF
	CALL k1011AddLikability(LIMIT(LOCAL * k1011GetCflag("好感") / 100, 100, 10000))
	CALL k1011AddJuel("恭顺", LOCAL * 1000)
	CALL k1011GWait()
	K1011DEventFlow:弹幕游戏 = 1
	
