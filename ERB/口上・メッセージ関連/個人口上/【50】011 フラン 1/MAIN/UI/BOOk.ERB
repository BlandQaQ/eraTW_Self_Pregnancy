@k1011BookColor, char, alpha, textAlpha = 0
	#FUNCTION
	#DIMS char
	#DIM alpha
	#DIM textAlpha
	#DIM color
	; DEBUGPRINTFORM %char%
	SELECTCASE char
		CASE "|", "□", "＝"
			color = 0x6e6500
			RETURNF alpha_color(color, alpha)
		CASE "＠"
			color = 0x000000
			RETURNF alpha_color(color, alpha)
		CASE "１", "："
			color = 0x747474
			RETURNF alpha_color(color, alpha)
		CASE "＃"
			color = 0xd7a67e
			RETURNF alpha_color(color, alpha)
		CASEELSE
			color = 0x000000
			RETURNF alpha_color(color, textAlpha)
	ENDSELECT
	
;逐行打印字符画内容
@k1011CharImgLinePrint, text, colorList, colorIndex
	#DIMS text
	#DIM REF colorList, 0, 0
	#DIM colorIndex
	#DIM _COUNT
	text = %REPLACE(text,"​","")%
	FOR _COUNT, 0, STRLENSU(text)
		SETCOLOR colorList:colorIndex:_COUNT
		PRINTS SUBSTRINGU(text, _COUNT, 1)
	NEXT
	PRINTL
