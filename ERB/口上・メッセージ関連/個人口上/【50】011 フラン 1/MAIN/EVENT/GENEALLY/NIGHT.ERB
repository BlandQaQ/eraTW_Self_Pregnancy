	#DIM kind
	;自慰
	IF kind == 1
		;恋慕时会嫉妒
		IF k1011GetTalent("恋慕")
			CALL k1011GPC("W", @"「呐、和%k1011Name(TARGET, TARGET)%一起玩吧、%k1011Name(TARGET, MASTER)%」", TARGET)
			CALL k1011GPC("W", @"「看、%k1011Name(TARGET, TARGET)%的小穴已经湿了……%k1011Name(TARGET, MASTER)%、%k1011Name(TARGET, TARGET)%想要了……」", TARGET)
			CALL k1011GPC("W", @"「胸部也很软……嘻嘻、嗯……可以随便玩哦……%k1011Name(TARGET, MASTER)%……」", TARGET)
		;除此之外
		ELSE
			CALL k1011GPC("W", @"「从小穴里发出了很多吧嗒吧嗒的声音……这样摸……」", TARGET)
			CALL k1011GPC("W", @"「……咦?为什麼%k1011Name(TARGET, TARGET)%的小穴已经这麼湿了呢……啊、哦、哦……」", TARGET)
		ENDIF
	ENDIF
