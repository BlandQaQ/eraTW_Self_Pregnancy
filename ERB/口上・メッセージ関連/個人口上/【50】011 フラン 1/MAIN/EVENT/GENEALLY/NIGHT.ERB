;陪睡事件
@k1011NightEventSleepWithMaster, scuess
	#DIM scuess
	IF scuess
		IF RAND:2 == 0
			CALL k1011GPC("W", @"「真的吗?能让%k1011Name(TARGET, TARGET)%睡到旁边吗、%k1011Name(TARGET, MASTER)%?嗯、太好了!」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%明天会把你叫起来作为谢礼、%k1011Name(TARGET, MASTER)%就先期待着吧、嘻嘻♪」", TARGET)
		ELSE
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%……嘿嘿……」", TARGET)
			CALL k1011GPC("W", @"「晚安!」", TARGET)
		ENDIF
	ELSE
		CALL k1011GPC("W", @"「……一个人待着、就会有这种心情」", TARGET)
		CALL k1011GPC("W", @"「......%k1011Name(TARGET, MASTER)%……好寂寞啊……」", TARGET)
	ENDIF

;相爱事件
@k1011NightEventLove(kind)
	#DIM kind
	SELECTCASE kind
		;夜宵(h)
		CASE 1
			CALL k1011GP("W", @"%CALLNAME:MASTER%正准备睡觉时、芙兰端着一碗汤来了")
			CALL k1011GP("W", @"似乎是汤太香了、%CALLNAME:MASTER%的睡意顿时被肚子的咕咕声取代了")
			CALL k1011GP("W", @"两人就这样头挤着头吃完了夜宵")
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%、夜宵好吃吗?」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%、芙兰会得到奖励吗?」", TARGET)
			CALL k1011GPC("W", @"「嗯、多多奖励%k1011Name(TARGET, TARGET)%吧!♪」", TARGET)
		;夜访(h)
		CASE 2
			CALL k1011GPC("W", @"「妻子的工作、就是要防止丈夫出轨、当然要多做色情的事情啦♪」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%%k1011Name(TARGET, TARGET)%已经准备好了、快点、快点啊♪」", TARGET)
		;陪睡
		CASE 3
			IF RAND:4 == 0
				CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%和%k1011Name(TARGET, TARGET)%结婚了……是因为%k1011Name(TARGET, MASTER)%喜欢萝莉吗?」", TARGET)
				CALL k1011GPC("W", @"「呵呵……嘿嘿……」", TARGET)
				CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%是萝莉控真好♪」", TARGET)
			ELSEIF RAND:3 == 0
				CALL k1011GPC("W", @"「呐%k1011Name(TARGET, MASTER)%、我们、真的结婚了呢…?」", TARGET)
				CALL k1011GPC("W", @"「总觉得、有时候…会有阵莫名的不安……」", TARGET)
				CALL k1011GP("W", @"为了消除芙兰的不安、%CALLNAME:MASTER%用力把芙兰抱在怀里")
				CALL k1011GPC("W", @"「啊……嗯……我们要、一直、一直、一直、一直、在一起……♪」", TARGET)
			ELSEIF RAND:2 == 0
				CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%是%k1011Name(TARGET, TARGET)%的、%k1011Name(TARGET, MASTER)%是%k1011Name(TARGET, TARGET)%的丈夫♪」", TARGET)
				CALL k1011GP("W", @"%CALLNAME:MASTER%的新婚妻子红着脸、兴奋得哇哇叫着")
			ELSE
				CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%、我们都是夫妻了、可以天天做色情的事情了♪」", TARGET)
				IF k1011GetTalent("怀孕")
					CALL k1011GPC("W", @"「等孩子出生后、%k1011Name(TARGET, TARGET)%还要继续做个色情的孩子♪」", TARGET)
				ELSE
					IF K1011HavePreg == 1
						CALL k1011GPC("W", @"「喂喂、我们再做些能让%k1011Name(TARGET, TARGET)%肚子变大的事情呗♪」", TARGET)
						CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%很高兴能有%k1011Name(TARGET, MASTER)%的孩子♪但%k1011Name(TARGET, TARGET)%还想再要一个」", TARGET)
					ELSE
						CALL k1011GPC("W", @"「夫妇俩好像是要生孩子的。但是要怎么做啊、%k1011Name(TARGET, MASTER)%?」", TARGET)
						CALL k1011GP("W", @"看着芙兰天真无邪地面容、%CALLNAME:MASTER%不知道该如何回答是好")
					ENDIF
				ENDIF
			ENDIF
	ENDSELECT

;眠奸MASTER
@k1011NightEventSleepH(kind)
	#DIM kind
	SELECTCASE kind
		;挿入失败(献处失败) 
		CASE 1
			CALL k1011GP("L", @"想到了前段时间%CALLNAME:MASTER%提到的做爱的意义、芙兰顿时涨红了脸蛋")
			CALL k1011GPC("W", @"「好想、好想……和%k1011Name(TARGET, MASTER)%结合……」", TARGET)
			CALL k1011GP("L", @"犹豫了一段时间后、悄悄的爬到了熟睡的%CALLNAME:MASTER%身上")
			CALL k1011GP("W", @"学着偷偷在书上看到的做爱姿势、张开了蜜穴、准备把%CALLNAME:MASTER%的东西吞下去")
			CALL k1011GP("W", @"但似乎由于没有经验、肉棒在蜜穴处滑了下就掉出来了")
			CALL k1011GPC("W", @"「……啊、我刚才想干什麼?」", TARGET)
			CALL k1011GPC("W", @"「好可怕啊、%k1011Name(TARGET, MASTER)%……」", TARGET)
		;逆强奸
		CASE 2
			CALL k1011GPC("W", @"「哦……%k1011Name(TARGET, MASTER)%的肉棒、要受不了了啊……」", TARGET)
			CALL k1011GPC("W", @"「喂、戳一下、%k1011Name(TARGET, TARGET)%的小穴呗……%k1011Name(TARGET, MASTER)%的肉棒看上去还能继续做呢♪」", TARGET)
		;口交
		CASE 3
			CALL k1011GPC("W", @"「啊、%k1011Name(TARGET, MASTER)%的肉棒……这个时候这麼可爱啊」", TARGET)
			CALL k1011GPC("W", @"「…………那个、还是用嘴做吧…♪」", TARGET)
			CALL k1011GPC("W", @"「%k1011Sound("Mouth", "啾、啾、哼……久、啾、啾。")%」", TARGET)
			CALL k1011GPC("W", @"「%k1011Sound("MouthShout", "嗯、嗯、嗯……!……喂!")%」", TARGET)
		;手淫
		CASE 4
			;消極的
			IF RAND:2 == 0
				CALL k1011GPC("W", @"「啊……嗯……%k1011Name(TARGET, MASTER)%的手比芙兰的大得多……」", TARGET)
			;積極的
			ELSE
				CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%的手、在芙兰的内裤里摸了……」", TARGET)
				CALL k1011GPC("W", @"「嗯?%k1011Name(TARGET, MASTER)%……」", TARGET)
			ENDIF
		;貞操帯
		CASE 5
			CALL k1011GPC("L", @"「嗯、如果%k1011Name(TARGET, MASTER)%醒来发现芙兰还好好带着贞操带的话」", TARGET)
			CALL k1011GPC("W", @"「一定会好好奖励%k1011Name(TARGET, TARGET)%的」", TARGET)
	ENDSELECT

;两人h
@k1011NightEventH(kind)
	#DIM kind
	SELECTCASE kind
		;消极。但主人要求
		CASE 1
			CALL k1011GPC("W", @"「不要!不要!」", TARGET)
			CALL k1011GPC("W", @"「色情的事情……现在芙兰不想做!」", TARGET)
		;消极
		CASE 2
			CALL k1011GPC("W", @"「那个、那个……」", TARGET)
			CALL k1011GPC("W", @"「……嗯、做吧」", TARGET)
		;积极
		CASE 3
			CALL k1011GPC("W", @"「喂、我们来做下色情的事吧~ ~」", TARGET)
			CALL k1011GPC("W", @"「不然的话、今天就睡不好觉了哦♪」", TARGET)
		;献处
		CASE 4
			CALL k1011GPC("W", @"「呜、呜……啊、%k1011Name(TARGET, MASTER)%……」", TARGET)
			CALL k1011GPC("W", @"「到现在为止、%k1011Name(TARGET, TARGET)%、一直、非常想和%k1011Name(TARGET, MASTER)%的関系、更进一步……」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%、终于和%k1011Name(TARGET, MASTER)%融为一体了……嗯、小穴把肉棒吞下去了…♪」", TARGET)
			CALL k1011GPC("W", @"「肉棒、好大、好热啊!」", TARGET)
			CALL k1011GP("W", @"看到芙兰坚强的微笑、%CALLNAME:MASTER%忘记了体验处女的价值、開始侵犯芙兰")
			CALL k1011GPC("W", @"「啊!啊!啊!好疼、好疼、不过%k1011Name(TARGET, TARGET)%很高兴!」", TARGET)
			CALL k1011GPC("W", @"「啊、肉棒、嗯、好大…好、烫、来把它插到%k1011Name(TARGET, TARGET)%身体里吧!♪」", TARGET)
	ENDSELECT

