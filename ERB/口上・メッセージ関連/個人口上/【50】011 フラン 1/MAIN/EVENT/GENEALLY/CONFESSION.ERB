@k1011NorConfession()
	CALL k1011GP("W", "芙兰的样子很奇怪")
	CALL k1011GP("W", "以往白皙的脸蛋上挂着开心的笑容与可爱的小虎牙、缓缓扑腾着小翅膀\n用宛如玩偶娃娃的小手抓着自己中指\n还时不时哒哒哒的跑到前面转身催干自己的芙兰酱今天竟然变得扭扭捏捏起来")
	CALL k1011GP("W", "芙兰的翅膀无力的耷拉到地上、步伐比往日慢了不少、还时不时停了下来\n洁白的脸蛋被不知道什么时候泛红起来、还时不时有汗珠从脸颊旁滑落")
	CALL k1011GP("W", "唔、这已经是第三次停下脚步了\n之前问她她都支支吾吾的回答没有事、这一定是在逞强吧?\n真让人担心还是赶快把她抱回家看看比较好")
	CALL k1011GP("W", "抱着这样的想法、你先生弯下蹲下身体来、张开双臂把少女娇小的身躯搂入了怀里")
	CALL k1011GP("W", "眼前目光在四周游走的少女深吸一口气、把脸蛋扭了过来、用似乎闪烁着某种光芒的鲜红的眼睛注视着自己")
	CALL k1011GPC("W", @"「那个、%k1011Name(TARGET, MASTER)%…%k1011Name(TARGET, TARGET)%、%k1011Name(TARGET, TARGET)%其实是想向%k1011Name(TARGET, MASTER)%告白」", TARGET)
	CALL k1011GPC("W", @"「…%k1011Name(TARGET, TARGET)%……%k1011Name(TARGET, TARGET)%以往都是自己一个人玩…但……但……但……」", TARGET)
	CALL k1011GP("W", "面前的少女的脸蛋变得更红了、宛如刚煮熟的红苹果一般、汗水宛如雨滴般打落到了自己的胳膊上\n看来是忘词了")
	CALL k1011GP("W", "举起了另一只手、缓缓抚摸过芙兰酱的脑袋、表示安慰")
	CALL k1011GP("W", "似乎是安慰取到了效果、眼前的少女重新做了一次深呼吸、吐出了一句话")
	CALL k1011GPC("W", @"「……总之…%k1011Name(TARGET, TARGET)%喜欢%k1011Name(TARGET, MASTER)%……%k1011Name(TARGET, TARGET)%想和%k1011Name(TARGET, MASTER)%在一起……」", TARGET)
