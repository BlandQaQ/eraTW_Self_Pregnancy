;记忆消除药
@k1011MemoryForget()
	;初次(YM特有,就用ym的判定吧)
	IF (CFLAG:17 & 16) == 0
		;看着好刀啊，我为什麼要在这里受罪。有没有这种药，给我也来个，好像把这个片段忘了
		IF k1011GetTalent("恋慕")
			CALL k1011GPC("W", @"「这次吃这个药就行了吧?」", TARGET)
			CALL k1011GP("W", @"已经不再怀疑%CALLNAME:MASTER%的芙兰毫不犹豫地吞下了那个药。")
			CALL k1011GPC("W", @"「嗯、有点苦吧。」", TARGET)
			CALL k1011GPC("W", @"「然后%k1011Name(TARGET, MASTER)%、这次玩什麼?」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%——?」", TARGET)
			CALL k1011GPC("W", @"「你就芙兰过来就是为了吃药吗?什麼嘛、真无聊……」", TARGET)
			CALL k1011GP("W", @"芙兰垂头丧气、身体突然塌了下来")
			CALL k1011GPC("W", @"「咦……总觉得有点迷糊……」", TARGET)
			CALL k1011GPC("W", @"「是困了吧……%k1011Name(TARGET, MASTER)%、芙兰稍微睡一觉也可以…?」", TARGET)
			CALL k1011GPC("W", @"「嗯、那就睡一会儿吧…」", TARGET)
			CALL k1011GPC("W", @"「我马上起来、然后——」", TARGET)
			CALL k1011GPC("W", @"「想和你玩、是吧……」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%．．．．．．」", TARGET)
		;除此之外
		ELSE
			CALL k1011GPC("W", @"「啊、这是什麼……」", TARGET)
			CALL k1011GPC("W", @"「好呆……芙兰不会消失吧?」", TARGET)
			CALL k1011GPC("W", @"「嗯……稍微睡一下。晚安、%k1011Name(TARGET, MASTER)%……」", TARGET)
		ENDIF
	;二次
	ELSE
		;記憶を無くす前は恋慕
		IF k1011GetTalent("恋慕")
			CALL k1011GPC("W", @"「这次吃这个药就行了吧?」", TARGET)
			CALL k1011GP("W", @"已经不再怀疑%CALLNAME:MASTER%的芙兰把那个药——")
			CALL k1011GPC("W", @"「啊…是?」", TARGET)
			CALL k1011GP("W", @"弄丢了")
			CALL k1011GPC("W", @"「为什麼呢?好像、好像……」", TARGET)
			CALL k1011GPC("W", @"「好伤心啊……」", TARGET)
			CALL k1011GPC("W", @"「唔、顺……顺……讨厌、我不想吃这个药!」", TARGET)
			CALL k1011GP("W", @"芙兰抱住%CALLNAME:MASTER%、放声大哭")
			CALL k1011GP("W", @"……")
			CALL k1011GP("W", @"．．．．．．")
			CALL k1011GP("W", @".．.")
		;除此之外
		ELSE
			CALL k1011GPC("W", @"「啊、这是什麼……」", TARGET)
			CALL k1011GPC("W", @"「好呆……芙兰不会消失吧?」", TARGET)
			CALL k1011GPC("W", @"「嗯……稍微睡一下。晚安、%k1011Name(TARGET, MASTER)%……」", TARGET)
		ENDIF
	ENDIF 
	;个人不喜欢记忆控制之类的药物
	CALL k1011MemoryForgetDp()

