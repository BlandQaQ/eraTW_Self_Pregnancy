;营養剂
@k1011Nutrient()
	;初次
	IF (K1011MedicineFirst:营养剂) == 0
		CALL k1011GPC("L", @"「当然可以用药哦」", TARGET)
		CALL k1011GPC("W", @"「但不是睡了一天就会恢復原样吗?」", TARGET)
		K1011MedicineFirst:营养剂 = 1
	;二次
	ELSE
		;恋慕+体力500未満
		IF k1011GetTalent("恋慕") && k1011GetBase("体力") < 500
			CALL k1011GPC("L", @"「哦……因为和%k1011Name(TARGET, MASTER)%做了很多、所以才摇摇晃晃的」", TARGET)
			CALL k1011GPC("W", @"「喝了药……马上就又可以和%k1011Name(TARGET, MASTER)%色色了♪」", TARGET)
		;恋慕
		ELSEIF k1011GetTalent("恋慕")
			CALL k1011GPC("L", @"「嗯……马上就会好起来了的」", TARGET)
			CALL k1011GPC("W", @"「我们可以做更多色情的事了、%k1011Name(TARGET, MASTER)%♪」", TARGET)
		;除此之外
		ELSE
			CALL k1011GPC("L", @"「啊、好起来了……」", TARGET)
			CALL k1011GPC("W", @"「你还没做够色情的事情啊?明天再说吧」", TARGET)
		ENDIF
	ENDIF

;扶她药
@k1011FutaMedicine(mod)
	#DIM mod
	;消去時
	IF mod == 1
		IF K1011Character == 1
			CALL k1011GPC("W", @"「啊、%k1011Name(TARGET, TARGET)%的肉棒怎么消失了?」", TARGET)
			CALL k1011GPC("W", @"「……不过、没肉棒也很好啊…♪」", TARGET)
		ELSE
			CALL k1011GPC("W", @"「啊、肉棒不见了」", TARGET)
			CALL k1011GPC("W", @"「让阴蒂突然长大、是很累很难受的」", TARGET)
			CALL k1011GPC("W", @"「但是能让%k1011Name(TARGET, MASTER)%高兴就行♪」", TARGET)
		ENDIF
		CALL k1011SetTalent("持久", 0)
	;付加時
	ELSEIF mod == 0
		;初次
		IF K1011MedicineFirst:扶她药 == 0
			CALL k1011GPC("W", @"「咦?什麼?」", TARGET)
			CALL k1011GP("W", @"呼噜…呼噜……")
			CALL k1011GPC("W", @"「呀!芙兰的小豆豆越来越大了! !」", TARGET)
			CALL k1011GP("W", @"- - -")
			IF K1011Character == 1
				CALL k1011GP("W", @"芙兰满脸通红地注视着从腹股沟处挺立的一根阴茎")
				CALL k1011GPC("W", @"「讨厌……这样太丢人了……」", TARGET)
			ELSE
				CALL k1011GPC("W", @"「你看、%k1011Name(TARGET, TARGET)%也长肉棒了♪」", TARGET)
				CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%变成男孩子了吗?」", TARGET)
				CALL k1011GPC("W", @"「啊哈哈、你看、用手一擦就变大了、好奇怪啊——」", TARGET)
			ENDIF
			K1011MedicineFirst:扶她药 = 1
		;二次
		ELSE
			;恋慕
			IF K1011Character == 1
				CALL k1011GPC("W", @"「啊、%k1011Name(TARGET, TARGET)%的豆豆又……长大了……」", TARGET)
				CALL k1011GPC("W", @"「又长出肉棒了…♪」", TARGET)
			ELSE
				CALL k1011GPC("W", @"「咦?为什麼%k1011Name(TARGET, TARGET)%会长出肉棒?」", TARGET)
				CALL k1011GPC("W", @"「啊、又来了?让小豆豆突然变大、很累的……」", TARGET)
			ENDIF
		ENDIF
		CALL k1011GetFutaNormally
	ENDIF

;豊胸剂
@k1011BreastUp()
	;初次
	IF K1011MedicineFirst:丰胸 == 0
		;恋慕
		IF k1011GetTalent("恋慕")
			CALL k1011GPC("L", @"「哇、%k1011Name(TARGET, TARGET)%的胸部变大了…?」", TARGET)
			CALL k1011GP("W", @"起初芙兰一脸困惑、但当她知道这是现实时、马上就雀跃起来。")
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%、摸一摸看!是真的吧?」", TARGET)
		;除此之外
		ELSE
			CALL k1011GPC("L", @"「咦……这是%k1011Name(TARGET, TARGET)%的乳房吧?」", TARGET)
			CALL k1011GPC("W", @"「啊、好想快点和那家伙比较一下啊♪」", TARGET)
		ENDIF
		K1011MedicineFirst:丰胸 = 1
	;二次
	ELSE
		SELECTCASE k1011GetTalent("胸部")
			;巨乳→爆乳
			CASE 3
				CALL k1011GPC("L", @"「嗯……胸部好重啊……」", TARGET)
				CALL k1011GPC("W", @"「肩膀酸痛就是这样来的……美铃说的」", TARGET)
			;普通→巨乳
			CASE 2
				CALL k1011GPC("W", @"「萝莉巨乳?怎麼回事?」", TARGET)
				CALL k1011GPC("W", @"「算了、%k1011Name(TARGET, MASTER)%快来看、胸部很有趣♪」", TARGET)
				CALL k1011GP("W", @"芙兰天真烂漫地捧着豊满的乳房玩耍着。")
				CALL k1011GPC("W", @"芙兰那张娃娃脸的巨乳、在手中「噗噗」地变了形、真是令人血脉膨胀的景象", TARGET)
			;贫乳→普通
			CASE 1
				CALL k1011GP("L", @"芙兰用手指按压着变大的乳房")
				CALL k1011GPC("W", @"「啊…喔喔胸部好柔软啊」", TARGET)
			;飞机场→贫乳
			CASE 0
				CALL k1011GPC("L", @"「啊…恢復原状了」", TARGET)
				CALL k1011GPC("W", @"「呵呵……这是芙兰原本的胸部……」", TARGET)
		ENDSELECT
	ENDIF

;缩胸剂
@k1011BreastDown()
	;初次
	IF K1011MedicineFirst:缩胸 == 0
		CALL k1011GPC("L", @"「咦、咦咦…?」", TARGET)
		CALL k1011GPC("W", @"「胸部…怎麼变小了呢…」", TARGET)
		K1011MedicineFirst:缩胸 = 1
	ENDIF

;止乳药
@k1011Lactifuge()
	;怀孕
	IF k1011GetTalent("恋慕") && k1011GetTalent("怀孕")
		CALL k1011GPC("W", @"「咦……奶……不出来吗?」", TARGET)
		CALL k1011GP("W", @"芙兰脸上的血色顿时消失了")
		CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%、怎麼办、怎麼办……生了孩子就得喂奶啊……!」", TARGET)
	;恋慕
	ELSEIF k1011GetTalent("恋慕")
		CALL k1011GPC("L", @"「啊、没奶了」", TARGET)
		CALL k1011GPC("W", @"「可是为什麼会出现奶呢?」", TARGET)
	;除此之外
	ELSE
		CALL k1011GPC("L", @"「啊、终于停奶了。」", TARGET)
		CALL k1011GPC("W", @"「是因为%CALLNAME:MASTER%把奶全挤出来了、所以就没有了吗?」", TARGET)
	ENDIF

