;记忆消除药数据保存处理
@k1011MemoryForgetDp()
	;选择重置区
	CALL k1011ChoiceInitVar

	IF K1011FlanHaveRod & 1p0
		CALL k1011GP("W", @"%CALLNAME:MASTER%从芙兰的房间里搜出来了振动棒")
		SIF k1011GetItem("振动棒")
			CALL k1011GP("W", @"不过自己手里已经有震动棒了,%CALLNAME:MASTER%只好把它给扔了")
		CALL k1011SetItem("振动棒", 1)
	ENDIF
	IF K1011FlanHaveRod & 1p1
		CALL k1011GP("W", @"%CALLNAME:MASTER%从芙兰的房间里搜出来了肛门振动棒")
		SIF k1011GetItem("肛门振动棒")
			CALL k1011GP("W", @"不过自己手里已经有肛门振动棒了,%CALLNAME:MASTER%只好把它给扔了")
		CALL k1011SetItem("肛门振动棒", 1)
	ENDIF
