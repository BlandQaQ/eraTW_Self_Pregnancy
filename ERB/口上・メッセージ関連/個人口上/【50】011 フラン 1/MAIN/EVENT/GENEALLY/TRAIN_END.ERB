;调教后双人运动
@k1011TrainEndV
	IF K1011AllowVampirism && RAND:8 >= 5
		CALL k1011GPC("W", @"「啊、啊、%k1011Name(TARGET, MASTER)%的血…想要…好想要!」", TARGET)
		CALL k1011GP("W", @"%CALLNAME:MASTER%让芙兰咬住了自己的脖子、同时用力的抽插着芙兰的嫩穴")
		CALL k1011GPC("W", @"「%k1011Sound("Orgasm", "呼、嗯!嗯……～～～～～! !")%」", TARGET)
		CALL k1011GP("W", @"因吸血带来的敏锐触感、将两人多次引向至高的巅峰")
		CALL k1011GP("W", @"%CALLNAME:MASTER%就像生命被吸走了一样、把所有的精液都注入给了芙兰")
		CALL k1011Vampirism
		CALL k1011AddExp("精液经验", 5)
	;怀孕
	ELSEIF k1011GetTalent("怀孕")
		IF RAND:3 == 0
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%!……用力一点!再用力一点哦!", TARGET)
			CALL k1011GP("W", @"芙兰用力一摇腰、扑腾扑腾地晃着翅膀、扭动着身体")
			CALL k1011GPC("W", @"「好多精子、%k1011Sound("Orgasm", "哇、好……哇!")%」", TARGET)
		ELSEIF RAND:2 == 0
			CALL k1011GP("W", @"看到身怀六甲的少女在怀中乱舞的样子、%CALLNAME:MASTER%决定更加激烈的晃动着腰")
			CALL k1011GPC("W", @"「%k1011Sound("Orgasm", "呜呜呜!啊!啊!啊! !")%」", TARGET)
		ELSE
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%……再往里面一点」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%想同时怀两次孕……」", TARGET)
		ENDIF
	;V感觉Lv4以上 吸血
	ELSEIF k1011GetAbl("Ｖ感觉") >= 3
		IF RAND:4 == 0
			CALL k1011GPC("W", @"「不要走啊……%k1011Name(TARGET, MASTER)%、再多做点吧……♪」", TARGET)
			CALL k1011P("", @"「你摸一下这里……我都快湿透了」", TARGET)
		ELSEIF RAND:3 == 0
			CALL k1011GPC("W", @"「啊、身体热得受不了了啊……」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%！、继续、继续、快把%k1011Name(TARGET, TARGET)%的小穴弄得乱七八糟!", TARGET)
			CALL k1011GPC("W", @"「%k1011Sound("Orgasm", "啊、呀!")%%k1011Name(TARGET, MASTER)%！！」", TARGET)
		ELSEIF RAND:2 == 0
			CALL k1011GPC("W", @"「和%k1011Name(TARGET, MASTER)%的这种事、心情会变好的♪」", TARGET)
			CALL k1011GPC("W", @"「%k1011Sound("Orgasm", "嗯……呼、嗯……哼……")%」", TARGET)
			CALL k1011GP("W", @"%CALLNAME:MASTER%每次刺进子宫口、幼小的阴道就会把阴茎勒上")
			CALL k1011GPC("W", @"「%k1011Sound("Orgasm", "啊、哦……啊!")%出来了…♪」", TARGET)
		ELSE
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%、…快一点!更快点哦!", TARGET)
			CALL k1011GP("W", @"芙兰用力摇动着腰、扑腾扑腾地摇着小翅膀、扭动着身体")
			CALL k1011GPC("W", @"「好多精子啊、%k1011Sound("Orgasm", "哇、好……哇!")%」", TARGET)
		ENDIF
	;除此之外
	ELSE
		CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%…啊、啊!%k1011Name(TARGET, TARGET)%……做的……很好吧」", TARGET)
	ENDIF

;调教后夜爬
@k1011TrainEndNight
	;吸血夜爬
	IF K1011AllowVampirism && RAND:8 >= 4
		CALL k1011GPC("W", @"「啊、啊、%k1011Name(TARGET, MASTER)%……%k1011Name(TARGET, TARGET)%好渴啊…」", TARGET)
		CALL k1011GP("W", @"芙兰用私处叼着阴茎、倒了个身、咬着%CALLNAME:MASTER%的脖子")
		CALL k1011GP("W", @"由于身体无力、%CALLNAME:MASTER%便把主动权交给了芙兰")
		CALL k1011GPC("W", @"「%k1011Sound("Mouth", "哈——嗯……嗯……嗯。")%」", TARGET)
		CALL k1011GP("W", @"芙兰吸血的同时開始剧烈地扭动腰肢")
		CALL k1011GP("W", @"%CALLNAME:MASTER%每隔一次呼吸、就会把大量的精液吐到芙兰体内")
		CALL k1011GPC("W", @"「喂、%k1011Name(TARGET, MASTER)%%k1011Sound("Orgasm", "喂、哟……嗯、呼——呼")%」", TARGET)
		CALL k1011Vampirism
		CALL k1011AddExp("精液经验", 8)
		K1011VampirismTimes ++
	;怀孕
	ELSEIF k1011GetTalent("怀孕")
		IF RAND:2 == 0
			CALL k1011GPC("W", @"「这样做的话、孩子会早点出生的……」", TARGET)
			CALL k1011GP("W", @"孕育着新生命的少女一边喘着粗气、一边拼命地寻求%CALLNAME:MASTER%的精液")
			CALL k1011GP("W", @"%CALLNAME:MASTER%一次又一次地向芙兰的阴道射了精")
		ELSE
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%……孩子怎麼还不出生啊……」", TARGET)
			CALL k1011GPC("W", @"「好寂寞啊……好寂寞啊……」", TARGET)
			CALL k1011GP("W", @"芙兰因寂寞流出了泪、但仍含着%CALLNAME:MASTER%的东西继续动着")
		ENDIF
	;除此之外
	ELSE
		IF RAND:5 == 0
			CALL k1011GPC("L", @"「呵呵、%k1011Name(TARGET, MASTER)%、不是%k1011Name(TARGET, TARGET)%不让你睡、\n在这样的夜晚、不做的话身体会很难受…♪」", TARGET)
			CALL k1011GPC("W", @"「所以啊、今晚要一直陪%k1011Name(TARGET, TARGET)%做爱哦♪」", TARGET)
			CALL k1011GP("W", @"芙兰带着可爱而又淫靡的微笑、用激烈的腰技吸允着%CALLNAME:MASTER%的东西")
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%、%k1011Name(TARGET, MASTER)%已经快要去了吗?真讨厌、再忍耐一下、再做继续点啊♪」", TARGET)
			CALL k1011GPC("L", @"「嗯!♪%k1011Name(TARGET, MASTER)%的肉棒、在里面咕嘟咕嘟的……」", TARGET)
			CALL k1011GPC("W", @"「被色情的%k1011Name(TARGET, TARGET)%搞兴奋了吗?那就没办法了♪」", TARGET)
		ELSEIF RAND:4 == 0
			CALL k1011GPC("W", @"「啊、啊…%k1011Name(TARGET, MASTER)%……身体热得睡不着」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%、芙兰好想去、小穴想去好多次!」", TARGET)
		ELSEIF RAND:3 == 0
			CALL k1011GPC("L", @"「今天、%k1011Name(TARGET, TARGET)%想吃肉棒……可以吗?」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%会和%k1011Name(TARGET, MASTER)%在一起的……」", TARGET)
			CALL k1011GP("W", @"两人一直運动到早上、芙兰始终在%CALLNAME:MASTER%身上晃动着小小的身体")
		ELSEIF RAND:2 == 0
			CALL k1011GPC("L", @"「%k1011Sound("Happy", "哈、啊……恩、恩、")%忍不下去了」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%一个人做了好几次、但没有%k1011Name(TARGET, MASTER)%的东西、根本满足不了……」", TARGET)
			CALL k1011GPC("W", @"「啊!继续啊、更用力的戳%k1011Name(TARGET, TARGET)%吧!♪」", TARGET)
			CALL k1011GPC("W", @"「哇、哇!变成%k1011Name(TARGET, TARGET)%的小穴肉棒的形状了哟!」", TARGET)
		ELSE
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%、%k1011Name(TARGET, TARGET)%很想和你在一起…所以就来了……」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%、可以的吧……」", TARGET)
		ENDIF
	ENDIF

