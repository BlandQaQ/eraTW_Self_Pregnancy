						CALL k1011SetTalent("胸部", 1)
				ENDSELECT
			;非恋慕
			ELSE
				SELECTCASE k1011GetTalent("胸部")
					;巨乳→爆乳
					CASE 3
						CALL k1011GPC("L", @"「嗯……最近胸部越来越重了……」", TARGET)
						CALL k1011GPC("W", @"「肩膀酸痛就是这样来的……美铃说的」", TARGET)
						CALL k1011GPC("W", @"「这就是帕秋莉说的胸可以被揉大吗?但这麼大太难受了」", TARGET)
						CALL k1011GP("W", @"＜芙兰的胸由[巨乳]变成了[爆乳]＞")
						CALL k1011SetTalent("胸部", 4)
					;普通→巨乳
					CASE 2
						CALL k1011GPC("W", @"「萝莉巨乳?怎麼回事?」", TARGET)
						CALL k1011GPC("W", @"「算了、胸部好像很有趣♪」", TARGET)
						CALL k1011GP("W", @"芙兰天真烂漫地捧着豊满的乳房玩耍着")
						CALL k1011GP("W", @"芙兰那张娃娃脸的巨乳、在手中「噗噗」地变了形、真是令人血脉膨胀的景象")
						CALL k1011GP("W", @"＜芙兰的胸由[普通乳]变成了[巨乳]＞")
						CALL k1011SetTalent("胸部", 3)
					;贫乳→普通
					CASE 1
						CALL k1011GP("L", @"芙兰用手指按压着变大的乳房")
						CALL k1011GPC("W", @"「揉了这麼长时间、总算变大了、看来帕秋莉说的是对的」", TARGET)
						CALL k1011GPC("W", @"「啊…喔喔胸部好柔软啊」", TARGET)
						CALL k1011GPC("W", @"「下次就不怕那家伙在%k1011Name(TARGET, TARGET)%面前显摆了」", TARGET)
						CALL k1011GP("W", @"＜芙兰的胸由[普通乳]变成了[巨乳]＞")
						CALL k1011SetTalent("胸部", 2)
					;飞机场→贫乳
					CASE 0
						CALL k1011GPC("L", @"「啊…总算长回去了」", TARGET)
						CALL k1011GPC("W", @"「呵呵……这是%k1011Name(TARGET, TARGET)%原本的胸部……」", TARGET)
						CALL k1011GPC("W", @"「下次让%k1011Name(TARGET, TARGET)%喝胸部变小药的话、%k1011Name(TARGET, TARGET)%就把你捏爆」", TARGET)
						CALL k1011GP("W", @"＜芙兰的胸由[飞机场]变成了[贫乳]＞")
						CALL k1011SetTalent("胸部", 1)
				ENDSELECT
			ENDIF
			;如果是在非恋慕的情况下长到贫乳、芙兰不认为是揉大的
			SIF k1011GetTalent("恋慕") || k1011GetTalent("胸部") != 1
				K1011BDevelopFirst = 1
		;之后
		ELSEIF k1011GetTalent("恋慕")
			;打赌
			IF K1011BBet == 1
				SELECTCASE k1011GetTalent("胸部")
					;巨乳→爆乳
					CASE 3
						CALL k1011GPC("L", @"「嗯……最近胸部越来越重了……」", TARGET)
						CALL k1011GPC("L", @"「看来胸部是真的可以被揉大啊」", TARGET)
						CALL k1011GPC("W", @"「但%k1011Name(TARGET, MASTER)%为什麼总是给%k1011Name(TARGET, TARGET)%喝胸部变得大药?」", TARGET)
						CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%好懒」", TARGET)
						CALL k1011GP("W", @"＜芙兰的胸由[巨乳]变成了[爆乳]＞")
						CALL k1011SetTalent("胸部", 4)
						K1011BBet = 0
					;普通→巨乳
					CASE 2
						CALL k1011GPC("W", @"「哇、%k1011Name(TARGET, TARGET)%的胸还能长大啊……这样叫萝莉巨乳是吧?」", TARGET)
						CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%、快看、胸部很有趣♪」", TARGET)
						CALL k1011GP("W", @"芙兰天真烂漫地捧着豊满的乳房玩耍着、直到%CALLNAME:MASTER%提起之前関于胸能不能被揉大的争论")
						CALL k1011GPC("W", @"「抱歉、%k1011Name(TARGET, MASTER)%、之前误会你了…作为赔偿、%k1011Name(TARGET, TARGET)%的胸就任你摆布吧♪」", TARGET)
						CALL k1011GP("W", @"芙兰那和娃娃脸一样大的巨乳、在%CALLNAME:MASTER%手中「噗噗」地变了形、真是令人血脉膨胀的景象")
						CALL k1011GP("W", @"＜芙兰的胸由[普通乳]变成了[巨乳]＞")
						CALL k1011SetTalent("胸部", 3)
					;贫乳→普通
					CASE 1
						CALL k1011GP("L", @"芙兰用手指按压着变大的乳房")
						CALL k1011GPC("W", @"「原来胸真的可以被揉大欸」", TARGET)
						CALL k1011GPC("W", @"「多谢%k1011Name(TARGET, MASTER)%、之前误会你了」", TARGET)
						CALL k1011GPC("W", @"「啊…喔喔胸部好柔软啊」", TARGET)
						CALL k1011GPC("W", @"「这样芙兰就可以更好的侍奉%k1011Name(TARGET, MASTER)%了、嘻嘻」", TARGET)
						CALL k1011GP("W", @"＜芙兰的胸由[普通乳]变成了[巨乳]＞")
						CALL k1011SetTalent("胸部", 2)
						K1011BBet = 0
					;飞机场→贫乳
					CASE 0
						CALL k1011GPC("L", @"「啊…总算长回去了」", TARGET)
						CALL k1011GPC("W", @"「呵呵……这是芙兰原本的胸部……」", TARGET)
						CALL k1011GPC("W", @"「以后不要再以测试胸能不能被揉大为理由、喂%k1011Name(TARGET, TARGET)%胸部变小的药了」", TARGET)
						CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%的胸是自己长回去的」", TARGET)
						CALL k1011GP("W", @"＜芙兰的胸由[飞机场]变成了[贫乳]＞")
						;芙兰不觉得你赢了
						CALL k1011SetTalent("胸部", 1)
				ENDSELECT
			;正常
			ELSE
				SELECTCASE k1011GetTalent("胸部")
					;巨乳→爆乳
					CASE 3
						CALL k1011GPC("L", @"「嗯……最近胸部越来越重了……」", TARGET)
						CALL k1011GPC("W", @"「肩膀酸痛就是这样来的……美铃说的」", TARGET)
						CALL k1011GPC("W", @"「但如果%k1011Name(TARGET, MASTER)%喜欢的话、就大丈夫♪」", TARGET)
						CALL k1011GP("W", @"＜芙兰的胸由[巨乳]变成了[爆乳]＞")
						CALL k1011SetTalent("胸部", 4)
					;普通→巨乳
					CASE 2
						CALL k1011GPC("W", @"「哇、%k1011Name(TARGET, TARGET)%的胸还能长大啊……这样叫萝莉巨乳是吧?」", TARGET)
						CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%、快看、胸部很有趣♪」", TARGET)
						CALL k1011GP("W", @"芙兰天真烂漫地捧着豊满的乳房玩耍着")
						CALL k1011GPC("W", @"芙兰那和娃娃脸一样大的巨乳、在手中「噗噗」地变了形、真是令人血脉膨胀的景象", TARGET)
						CALL k1011GP("W", @"＜芙兰的胸由[普通乳]变成了[巨乳]＞")
						CALL k1011SetTalent("胸部", 3)
					;贫乳→普通
					CASE 1
						CALL k1011GP("L", @"芙兰用手指按压着变大的乳房")
						CALL k1011GPC("W", @"「揉了这麼长时间、总算变大了、多亏了%k1011Name(TARGET, MASTER)%的按摩」", TARGET)
						CALL k1011GPC("W", @"「啊…喔喔胸部好柔软啊」", TARGET)
						CALL k1011GPC("W", @"「这样%k1011Name(TARGET, TARGET)%就可以更好的侍奉%k1011Name(TARGET, MASTER)%了、嘻嘻」", TARGET)
						CALL k1011GP("W", @"＜芙兰的胸由[普通乳]变成了[巨乳]＞")
						CALL k1011SetTalent("胸部", 2)
					;飞机场→贫乳
					CASE 0
						CALL k1011GPC("L", @"「啊…总算长回去了」", TARGET)
						CALL k1011GPC("W", @"「呵呵……这是%k1011Name(TARGET, TARGET)%原本的胸部……」", TARGET)
						CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%以后不要在喂%k1011Name(TARGET, TARGET)%让胸变小的药了、长回去是很不容易的」", TARGET)
						CALL k1011GP("W", @"＜芙兰的胸由[飞机场]变成了[贫乳]＞")
						CALL k1011SetTalent("胸部", 1)
				ENDSELECT
			ENDIF
		;除此之外
		ELSE
			SELECTCASE k1011GetTalent("胸部")
				;巨乳→爆乳
				CASE 3
					CALL k1011GPC("L", @"「嗯……最近胸部越来越重了……」", TARGET)
					CALL k1011GPC("W", @"「肩膀酸痛就是这样来的……美铃说的」", TARGET)
					CALL k1011GP("W", @"＜芙兰的胸由[巨乳]变成了[爆乳]＞")
					CALL k1011SetTalent("胸部", 4)
				;普通→巨乳
				CASE 2
					CALL k1011GPC("W", @"「萝莉巨乳?怎麼回事?」", TARGET)
					CALL k1011GPC("W", @"「算了、胸部好像很有趣♪」", TARGET)
					CALL k1011GP("W", @"芙兰天真烂漫地捧着豊满的乳房玩耍着")
					CALL k1011GPC("W", @"芙兰那张娃娃脸的巨乳、在手中「噗噗」地变了形、真是令人血脉膨胀的景象", TARGET)
					CALL k1011GP("W", @"＜芙兰的胸由[普通乳]变成了[巨乳]＞")
					CALL k1011SetTalent("胸部", 3)
				;贫乳→普通
				CASE 1
					CALL k1011GP("L", @"芙兰用手指按压着变大的乳房")
					CALL k1011GPC("W", @"「揉了这麼长时间、总算变大了、帕秋莉说的是对的」", TARGET)
					CALL k1011GPC("W", @"「啊…喔喔胸部好柔软啊」", TARGET)
					CALL k1011GPC("W", @"「到时候找姐姐大人炫耀炫耀」", TARGET)
					CALL k1011GP("W", @"＜芙兰的胸由[普通乳]变成了[巨乳]＞")
					CALL k1011SetTalent("胸部", 2)
				;飞机场→贫乳
				CASE 0
					CALL k1011GPC("L", @"「啊…总算长回去了」", TARGET)
					CALL k1011GPC("W", @"「呵呵……这是%k1011Name(TARGET, TARGET)%原本的胸部……」", TARGET)
					CALL k1011GPC("W", @"「看在你辛辛苦苦按摩的份上、就不给你算让我喝胸部变小药的账了」", TARGET)
					CALL k1011GP("W", @"＜芙兰的胸由[飞机场]变成了[贫乳]＞")
					CALL k1011SetTalent("胸部", 1)
			ENDSELECT
		ENDIF
		;揉胸进度归零
		K1011BDevelopJuel = 0
		;胸揉大
		RETURN 1
	ENDIF
	;胸没揉大
	RETURN 0

;-------------------------------------------
;胸部停止生长口上
;揉胸进度达到1000触发
;-------------------------------------------
@k1011BNotDevelop
	;首次
	IF !K1011BDevelopStopFirst && K1011BDevelopJuel > 1000
		DRAWLINE
		;被揉大过胸
		IF K1011BDevelopFirst
			IF k1011GetTalent("恋慕")
				SELECTCASE k1011GetTalent("胸部")
					;爆乳
					CASE 4
						CALL k1011GPC("W", @"「明明%k1011Name(TARGET, MASTER)%都揉了这麼长时间、胸部怎麼还不长大啊?」", TARGET)
						CALL k1011GP("W", @"%CALLNAME:MASTER%摸了摸芙兰的小脑袋、表示胸已经达到了生物的极限")
						CALL k1011GPC("W", @"「不要、%k1011Name(TARGET, TARGET)%要打破极限」", TARGET)
						CALL k1011GP("W", @"贪得无厌的芙兰抱着和幼小躯体不相符的爆乳向%CALLNAME:MASTER%抱怨到")
					;巨乳
					CASE 3
						CALL k1011GPC("W", @"「明明%k1011Name(TARGET, MASTER)%都揉了这麼长时间、胸部怎麼还不长大啊?」", TARGET)
						CALL k1011GP("W", @"%CALLNAME:MASTER%摸了摸芙兰的小脑袋、表示胸已经大的离谱了")
						CALL k1011GPC("W", @"「不行、%k1011Name(TARGET, TARGET)%的胸要比门番还大才行」", TARGET)
						CALL k1011GPC("W", @"「所以%k1011Name(TARGET, MASTER)%要多揉揉%k1011Name(TARGET, TARGET)%的胸哦♪」", TARGET)
						CALL k1011GP("W", @"仍不死心的芙兰拿着大的离谱的胸、往%CALLNAME:MASTER%身上蹭着")
					;普通
					CASE 2
						CALL k1011GPC("W", @"「胸怎麼不生长呢?明明%k1011Name(TARGET, MASTER)%的手法明明和以前一样……」", TARGET)
						CALL k1011GPC("W", @"「嗯、我和%k1011Name(TARGET, MASTER)%一起揉」", TARGET)
						CALL k1011GP("W", @"芙兰把霊巧的小手伸向了胸部、和%CALLNAME:MASTER%一起揉捏着")
						CALL k1011GPC("W", @"「嗯……这样应该能长大了吧……」", TARGET)
					;贫乳
					CASE 1
						CALL k1011GPC("W", @"「都揉了这麼长时间了、胸部都没再长大、所以%k1011Name(TARGET, TARGET)%的胸是自己长起来的」", TARGET)
						CALL k1011GP("W", @"作为争论的胜利者、芙兰没有半点開心的样子、反而靠在%CALLNAME:MASTER%身上")
						CALL k1011GPC("W", @"「……%k1011Name(TARGET, TARGET)%的胸……会不会只能长这麼大了…………」", TARGET)
						CALL k1011GP("W", @"%CALLNAME:MASTER%温柔的抱住了芙兰、摸摸小脑袋以作安慰、表示有空了去买豊胸剂")
					;飞机场
					CASE 0
				ENDSELECT
				CALL k1011GWait()
				CALL k1011GP("W", @"看来芙兰的胸部的潜力已经耗尽了、想要长得更大、只能依靠豊胸剂了")
			ELSE
				SELECTCASE k1011GetTalent("胸部")
						;爆乳
						CASE 4
							CALL k1011GPC("W", @"「明明揉了这麼长时间、胸部怎麼还不长大啊?」", TARGET)
							CALL k1011GP("W", @"贪得无厌的芙兰抱着和幼小躯体不相符的爆乳抱怨到")
						;巨乳
						CASE 3
							CALL k1011GPC("W", @"「已经揉了这麼长时间、胸怎麼不生长呢?」", TARGET)
							CALL k1011GPC("W", @"「什麼时候胸部才能比门番大呢?」", TARGET)
							CALL k1011GP("W", @"贪得无厌的芙兰抱着和幼小躯体不相符的巨乳抱怨到")
						;普通
						CASE 2
							CALL k1011GPC("W", @"「胸怎麼不生长呢?一定是%k1011Name(TARGET, MASTER)%没有好好揉……大不了我自己做」", TARGET)
							CALL k1011GP("W", @"芙兰一边自言自语着、一边用力揉着乳房")
							CALL k1011GPC("W", @"「嗯……这样应该能长大了吧……」", TARGET)
						;贫乳
						CASE 1
						;飞机场
						CASE 0
				ENDSELECT
				IF k1011GetTalent("胸部") == 4
					CALL k1011GWait()
					CALL k1011GP("W", @"看来芙兰的胸部已经张到了极限、不能再发育了")
