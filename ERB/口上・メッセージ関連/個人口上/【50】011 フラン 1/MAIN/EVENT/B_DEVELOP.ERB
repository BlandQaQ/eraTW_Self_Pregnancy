						CALL k1011GPC("W", @"「这样%k1011Name(TARGET, TARGET)%就可以更好的侍奉%k1011Name(TARGET, MASTER)%了、嘻嘻」", TARGET)
						CALL k1011GP("W", @"＜芙兰的胸由[普通乳]变成了[巨乳]＞")
						CALL k1011SetTalent("胸部", 2)
					;飞机场→贫乳
					CASE 0
						CALL k1011GPC("W", @"「啊…总算长回去了」", TARGET)
						CALL k1011GPC("W", @"「什麼吗……这和%k1011Name(TARGET, MASTER)%揉胸没什麼関系吧、%k1011Name(TARGET, TARGET)%的胸本来就这麼大的」", TARGET)
						CALL k1011GP("W", @"%CALLNAME:MASTER%与芙兰争执半天也每个结论、最后决定以事实说话")
