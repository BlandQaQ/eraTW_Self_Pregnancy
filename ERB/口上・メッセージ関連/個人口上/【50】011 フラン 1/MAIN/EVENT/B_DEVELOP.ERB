;===================================================================
;胸长大事件
;===================================================================
;胸部生长判定
;胸部生长口上
;胸部停止生长口上

;-------------------------------------------
;胸部生长判定
;-------------------------------------------
@k1011BCanDevelop
	#FUNCTION
	;胸部大小小于设定值
	SIF k1011GetTalent("胸部") < K1011Config:bDevMax
		RETURNF 1
	RETURNF 0

;-------------------------------------------
;胸部生长口上
;-------------------------------------------
@k1011BDevelop
	;胸可以生长,揉胸进度达到500
	IF k1011BCanDevelop() && K1011BDevelopJuel >= 500
		DRAWLINE
		CALL k1011GP("W", @"在%CALLNAME:MASTER%的揉捏下、芙兰的胸部长大了")
		CALL k1011GWait()
		;第一次
		IF K1011BDevelopFirst == 0
			IF k1011GetTalent("恋慕")
				;巨乳→爆乳
				SELECTCASE k1011GetTalent("胸部")
					CASE 3
						CALL k1011GPC("L", @"「嗯……最近胸部越来越重了……」", TARGET)
						CALL k1011GPC("W", @"「肩膀酸痛就是这样来的……美铃说的」", TARGET)
						CALL k1011GalSetMood("Confuse")
						CALL k1011GPC("W", @"「但是、最近%k1011Name(TARGET, MASTER)%也没有给我吃胸部变大的药啊……」", TARGET)
						CALL k1011GalSetMood("")
						CALL k1011GPC("W", @"「是被%k1011Name(TARGET, MASTER)%揉大的吗?」", TARGET)
						CALL k1011GalSetMood("Sing")
						CALL k1011GPC("W", @"「太好了、%k1011Name(TARGET, TARGET)%的胸上有%k1011Name(TARGET, MASTER)%的印记了♪」", TARGET)
						CALL k1011GalSetMood("")
						CALL k1011GP("W", @"芙兰一脸幸福的揉着因%CALLNAME:MASTER%而变大的胸")
						CALL k1011GPC("W", @"大的不正常的胸部在芙兰手中「噗噗」地变了形、真是令人血脉膨胀的景象", TARGET)
						CALL k1011GP("W", @"＜芙兰的胸由[巨乳]变成了[爆乳]＞")
						CALL k1011GP("W", @"＜芙兰的胸部长到了最大尺寸＞")
						CALL k1011SetTalent("胸部", 4)
					;普通→巨乳
					CASE 2
						CALL k1011GalSetMood("Happy")
						CALL k1011GPC("W", @"「哇、芙兰的胸又长大了啊……」", TARGET)
						CALL k1011GalSetMood("Confuse")
						CALL k1011GPC("W", @"「但是、最近%k1011Name(TARGET, MASTER)%也没有给我吃胸部变大的药啊……?」", TARGET)
						CALL k1011GalSetMood("")
						CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%知道了、这是%k1011Name(TARGET, MASTER)%按摩的结果」", TARGET)
						CALL k1011GalSetMood("Sing")
						CALL k1011GPC("W", @"「快来享受努力的成果吧♪」", TARGET)
						CALL k1011GalSetMood("")
						CALL k1011GP("W", @"芙兰天真烂漫地把%CALLNAME:MASTER%充满淫秽的手抓像豊满的乳房")
						CALL k1011GP("W", @"芙兰那和娃娃脸一样大的巨乳、在%CALLNAME:MASTER%手中「噗噗」地变了形、真是令人血脉膨胀的景象")
						CALL k1011GP("W", @"＜芙兰的胸由[普通乳]变成了[巨乳]＞")
						CALL k1011SetTalent("胸部", 3)
					;贫乳→普通
					CASE 1
						CALL k1011GP("L", @"芙兰用手指按压着变大的乳房")
						CALL k1011GPC("W", @"「揉了这麼长时间、总算变大了、原来%k1011Name(TARGET, MASTER)%的按摩是有效的」", TARGET)
