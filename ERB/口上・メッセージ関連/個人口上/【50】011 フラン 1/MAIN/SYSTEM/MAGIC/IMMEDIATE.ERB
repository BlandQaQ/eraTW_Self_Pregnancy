;排卵
@k1011Magic2
	#DIM DYNAMIC alpha, 3
	#DIM DYNAMIC mod
	#DIM DYNAMIC page
	#DIM DYNAMIC newPage
	#DIM DYNAMIC maxPage
	#DIM DYNAMIC out
	#DIM choice
	#DIM line
	#DIM index
	;统计信息
	line = LINECOUNT
	CALL k1011AlphaListClear
	CALL k1011AlphaListSetStrFormat, "NormallyCharaList"
	FOR index, 0, CHARANUM
		SIF !(k1011GetTalent("性别", index) & 1) && !k1011GetOtherInfo("未经初潮", index) && !(k1011GetTalent("卵生", index) || k1011GetTalent("产卵体质", index)) && !k1011GetTalent("怀孕") && !k1011GetTalent("抱卵中") && !k1011GetTalent("怀卵") && (K1011Game == "TW" || CFLAG:index:4 == 0)
			CALL k1011AlphaListAppend(index)
	NEXT
	;计算最大页数
	maxPage = k1011AlphaListMaxData() / 20
	SIF k1011AlphaListMaxData() % 20
		maxPage += 1
	;没有可施法对象
	IF maxPage == 0
		CALL k1011Warning, "没有可发动促孕魔法的对象"
		RETURN
	ENDIF
	WHILE 1
		CALL a_resetcolor, alpha
		DRAWLINE
		PRINTFORML 选择促孕魔法的发动对象  ＜page.{page + 1}/{maxPage}＞
		DRAWLINE
		CALL k1011AlphaListPrint, page * 20, (page + 1) * 20, alpha:1
		CALL a_resetcolor, alpha
		DRAWLINE
		CALL k1011ConditionButton, "[99997] 上一页", page > 0, 0
		PRINT     
		PRINT [99998] 退出
		PRINT     
		CALL k1011ConditionButton, "[99999] 下一页", page < (maxPage - 1), 0
		PRINTL
		SELECTCASE mod
			;出现
			CASE 0
				IF cartoon_play()
					alpha += 50
					alpha:1 = alpha
					SIF alpha > 255
						mod = 4
				ELSE
					alpha += 255
					alpha:1 = alpha
					mod = 4
				ENDIF
			;选项出现
			CASE 1
				IF cartoon_play()
					alpha:1 += 50
					SIF alpha:1 > 255
						mod = 4
				ELSE
					alpha:1 = 255
					mod = 4
				ENDIF
			;选项更新
			CASE 2
				IF cartoon_play()
					alpha:1 -= 50
					IF alpha:1 <= 0
						page = newPage
						mod = 1
					ENDIF
				ELSE
					page = newPage
					alpha:1 = 255
					mod = 4
				ENDIF
			;退出
			CASE 3
				IF cartoon_play()
					alpha -= 50
					alpha:1 = alpha
					SIF alpha <= 0
						BREAK
				ELSE
					BREAK
				ENDIF
			;等待
			CASE 4
		ENDSELECT
		TINPUT 15, -1, 0,
		choice = RESULT
		SELECTCASE choice
			CASE 0 TO (CHARANUM - 1)
				IF k1011AlphaListExist(choice)
					CALL k1011AskYn, @"要对%CALLNAME:choice%发动促孕魔法吗?"
					IF RESULT
						mod = 3
						out = 1
						CALL k1011SetTequip("排卵诱发剂", 720)
					ENDIF
				ENDIF
			;上一页
			CASE 99997
				IF newPage > 0
					newPage --
					mod = 2
				ENDIF
			CASE 99998
				mod = 3
			;下一页
			CASE 99999
				IF newPage < (maxPage - 1)
					newPage ++
					mod = 2
				ENDIF
		ENDSELECT
		CLEARLINE LINECOUNT - line
	WEND
	CLEARLINE LINECOUNT - line
	RETURN out

