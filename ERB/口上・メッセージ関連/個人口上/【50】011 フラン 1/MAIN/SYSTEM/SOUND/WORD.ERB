;痛苦标点符号
@k1011PainSoundMark
	#FUNCTIONS
	#DIMS wordList, 99
	#DIM weightList, 99
	#DIM DYNAMIC index
	#DIMS DYNAMIC out
	wordList:index = 、
	weightList:index = 20
	index ++

	wordList:index = ——
	weightList:index = 2
	index ++

	wordList:index = ……
	weightList:index = 5
	index ++

	SIF RAND:(20 + k1011GetAbl("受虐感")) > 20
		out += "♪"
	out += wordList:(k1011WRand(weightList))

	RETURNF out

