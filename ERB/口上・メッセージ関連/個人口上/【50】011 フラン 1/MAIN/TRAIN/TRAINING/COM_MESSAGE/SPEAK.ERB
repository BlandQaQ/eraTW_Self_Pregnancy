;对话主函数
@k1011ComMessageSpeakMain()
	#DIMS DYNAMIC funNames, 99
	#DIM DYNAMIC weight, 99
	#DIM DYNAMIC num
	#DIM scuess
	;太阳
	funNames:num = Sun
	weight:num = 10
	num ++
	;群星
	funNames:num = Stellaris
	weight:num = 10
	num ++
	;番茄炒鸡蛋
	funNames:num = TomatoWithEgg
	weight:num = 10
	num ++
	;变成蝙蝠的感觉
	funNames:num = FeelingOfBat
	weight:num = 10
	num ++
	;芙兰变得蝙蝠是什么种类
	funNames:num = KindOfBat
	weight:num = 10
	num ++
	;毁灭月球
	funNames:num = DestoryMoon
	weight:num = 10
	num ++
	;四重存在
	funNames:num = FourOfAKind
	weight:num = 10
	num ++
	;打则
	funNames:num = Daze
	weight:num = 10
	num ++
	;便当
	funNames:num = Bento
	weight:num = 10
	num ++
	;一天洗几次澡
	funNames:num = Bathe
	weight:num = 10
	num ++
	;最喜欢的玩具
	funNames:num = Toy
	weight:num = 10
	num ++
	;最喜欢的食物
	funNames:num = Food
	weight:num = 10
	num ++
	;问翅膀
	funNames:num = Wing
	weight:num = 10
	num ++
	;以前待在地下室的时候在做什么
	funNames:num = Life
	weight:num = 10
	num ++
	;喜欢的衣服
	funNames:num = Clothes
	weight:num = 10
	num ++
	;如何玩电子stg
	IF K1011DEventFlow:车万游戏 > 0
		funNames:num = HowToPlayEStg
		weight:num = 10
		num ++
	ENDIF

	;调用函数
	CALLFORM k1011ComMessageSpeak%funNames:k1011WRand(weight)%()

;太阳
@k1011ComMessageSpeakSun()
	CALL k1011GPC("W", @"「太阳吗……%k1011Name(TARGET, TARGET)%也不是很还怕捏」", TARGET)
	CALL k1011GPC("W", @"「毕竟太阳的光芒还没莱瓦汀耀眼捏」", TARGET)
	IF k1011GetTalent("疯狂")
		CALL k1011GP("W", @"眼前的少女一伸手、一团火焰毫无征兆的炸开、火苗差点飞溅到自己身上")
	ELSE
		CALL k1011GP("W", @"眼前的少女伸开手、一团火苗跃然手上、随后不断拉长变成了一把剑的形状")
	ENDIF
	;白天
	IF K1011_IS_DAY
		CALL k1011GP("W", @"定眼看看、这东西确实比天上的太阳的耀眼、魔法少女的力量可真是神奇")
	;晚上
	ELSE
		CALL k1011GP("W", @"在这团火焰的照耀下、周围的一切顿时亮如白昼、不、甚至比中午还要亮")
	ENDIF
	CALL k1011GPC("W", @"「不过%k1011Name(TARGET, TARGET)%还是很怕紫外线的、被照到话皮肤就会变黑、需要过很长时间才会恢复」", TARGET)
	CALL k1011GPC("W", @"「皮肤可是女孩子的生命、所以说、特制的太阳伞是必须的」", TARGET)

;群星
@k1011ComMessageSpeakStellaris()
	CALL k1011GPC("W", @"「『群星』?那是什么啊?」", TARGET)
	CALL k1011GP("W", @"%CALLNAME:MASTER%向睁着好奇大眼睛的芙兰酱介绍了外界的一款游戏、向她讲述了做屋里可以一连玩上几天的星系战争游戏")
	CALL k1011GPC("W", @"「原来是外界的游戏啊、你们外界人可真会玩、被关到屋里都能玩的那么起兴」", TARGET)
	CALL k1011GPC("W", @"「要是%k1011Name(TARGET, TARGET)%被关屋子里时也能玩到就好了……」", TARGET)
	CALL k1011GP("W", @"眼前的少女的翅膀无力的耷拉着、似乎想到了某段不开心的经理")
	IF k1011GetTalent("恋慕")
		CALL k1011GPC("W", @"「算了、过去的事情已经不重要了、现在%k1011Name(TARGET, TARGET)%有%k1011Name(TARGET, MASTER)%陪着」", TARGET)
		CALL k1011GP("W", @"面前的少女很快调整了过来、抬起头给了自己一个幸福的微笑")
	ENDIF
	CALL k1011GPC("W", @"「不过%k1011Name(TARGET, TARGET)%似乎在哪里听说过相似的东西」", TARGET)
	CALL k1011GPC("W", @"「好像是前段时间地下室飘进来了一张『文文日报』」", TARGET)
	CALL k1011GPC("W", @"「上面说月都那边也有人来自群星什么的……」", TARGET)
	CALL k1011GPC("W", @"「所以说、%k1011Name(TARGET, MASTER)%不会也是月都人吧?」", TARGET)

;番茄炒鸡蛋
@k1011ComMessageSpeakTomatoWithEgg()
	CALL k1011GPC("W", @"「番茄炒鸡蛋?那是什么?」", TARGET)
	CALL k1011GP("W", @"%CALLNAME:MASTER%露出了坏笑、指向了带着好奇的眼睛的芙兰酱")
	CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%是芙兰、才不是什么番茄炒鸡蛋、%k1011Name(TARGET, MASTER)%最坏了」", TARGET)
	CALL k1011GP("W", @"被挑逗的幼女撅着小嘴、用玩偶般精巧的小手捶打着自己的胸口")
	CALL k1011GP("W", @"被幼女缠的没法子、%CALLNAME:MASTER%只好向芙兰交代了什么是番茄炒鸡蛋")
	CALL k1011GPC("W", @"「红色的?有股淡淡的甜味?是咲夜没做过的食物捏」", TARGET)
	CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%想尝尝、%k1011Name(TARGET, MASTER)%能不能给%k1011Name(TARGET, TARGET)%做一份啊?」", TARGET)
	CALL k1011GP("W", @"面前的少女嘴边流出了一丝口水、用闪着期待光芒的大眼睛注视着你自己……")

;变成蝙蝠的感觉
@k1011ComMessageSpeakFeelingOfBat()
	CALL k1011GP("W", @"变成蝙蝠群的话意识是怎么保存的呢?\n这个问题可能自己永远都搞不明白、但问问眼前的吸血鬼就会立刻得出答案")
	CALL k1011GPC("W", @"「蝙蝠群嘛、你们控制身体上这么多细胞是什么感觉、控制蝙蝠群就什么感觉」", TARGET)
	CALL k1011GPC("W", @"「脑中想到让他们怎么做就会怎么做」", TARGET)
	CALL k1011GPC("W", @"「唔……听起来给玩rts游戏一样、吸血鬼可真是奇妙」", MASTER)
	CALL k1011GPC("W", @"「rts游戏?那是什么游戏?%k1011Name(TARGET, MASTER)%、快告诉%k1011Name(TARGET, TARGET)%呗」", TARGET)
	CALL k1011GP("W", @"一提起游戏、眼前的吸血鬼少女马上就打起了十二分精神、扑腾着小翅膀、用好奇的大眼睛看着你")

;芙兰变得蝙蝠是什么种类
@k1011ComMessageSpeakKindOfBat()
	CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%变得是什么蝙蝠?%k1011Name(TARGET, MASTER)%的问题可真的多啊」", TARGET)
	;恋慕
	IF k1011GetTalent("恋慕")
		CALL k1011GPC("W", @"「这么刨根问底的来盘问女孩子的身份、可是不太礼貌哦」", TARGET)
		CALL k1011GP("W", @"面前的少女稍稍崛起了嘴唇、接着又露出了一个笑脸")
		CALL k1011GPC("W", @"「如果是%k1011Name(TARGET, MASTER)%的话、就大丈夫♪」", TARGET)
	ENDIF
	CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%可是世界上最凶残最吓人的吸血蝙蝠哦」", TARGET)
	CALL k1011GP("W", @"眼前的少女一边一边敲着二郎腿、飞到半空中、摆出了统治者俯视万物的姿态")
	CALL k1011GP("W", @"接着看到少女的背后闪过一阵暗红色的光芒、几只嗜血蝙蝠飞到了自己身边盘旋着\n血红色的眼睛表明了这些蝙蝠的危险程度")
	CALL k1011GP("W", @"担当自己把藏到口袋的即时草莓布丁拿出来时、一切都改变了")
	CALL k1011GP("W", @"看似危险的吸血蝙蝠纷纷飞到了自己怀里、宛如乖乖的小兔子吃着布丁\n天上的芙兰酱那种高高在上的感觉瞬间消失了、化成了抢食物吃的小猫也扑进了自己的怀里")
	CALL k1011GPC("W", @"「原来%k1011Name(MASTER, TARGET)%是靠卖萌赚布丁吃的布丁蝠啊」", MASTER)
	CALL k1011GPC("W", @"「呜呜呜、%k1011Name(TARGET, TARGET)%拆不素森么普丁蝠、唔唔、%k1011Name(TARGET, TARGET)%素追可怕的四血片蝠……」", TARGET)
	CALL k1011GP("W", @"少女一边吃着布丁、一边为了维护自己的名誉发出了呜呜咽咽的声音")

;如何玩电子stg
@k1011ComMessageSpeakHowToPlayEStg()
	;TODO临时教师装
	CALL k1011GP("W", @"看到上次芙兰酱打stg游戏这么厉害、或许可以趁机学习下、说不定就可以摆脱被朋友贴上去的stg苦手的标签")
	CALL k1011GPC("W", @"「这是在向%k1011Name(TARGET, TARGET)%学习的吗?」", TARGET)
	CALL k1011GPC("W", @"「太棒了、%k1011Name(TARGET, TARGET)%当老师了」", TARGET)
	CALL k1011GP("W", @"眼前的少女带着玩过家家的兴奋感、带上了不知道从哪里搞来的眼睛")
	CALL k1011GPC("W", @"「听好了、%k1011Name(TARGET, MASTER)%同学」", TARGET)
	CALL k1011GPC("W", @"「stg其实很简单、你打的时候集中注意力一些、周围的东西速度都会变慢几倍、躲弹幕、擦弹的难度就变低了」", TARGET)
	IF k1011IsVampirism(MASTER)
		CALL k1011GP("W", @"变慢几倍?为啥自己打得时候连半倍都没有……芙兰这挂也太厉害了啊")
	ELSE
		CALL k1011GP("W", @"哈、集中注意力还会有子弹时间……原来吸血鬼自带外挂啊……")
	ENDIF
	CALL k1011GPC("W", @"「好了、课到这里就结束了、今天的作业就是禁b无伤打通LED扫地机、明天检查」", TARGET)
	CALL k1011GP("W", @"哎等下、还没给我讲明白该怎么打stg啊")

;毁灭月球
@k1011ComMessageSpeakDestoryMoon()
	CALL k1011GP("W", @"粗看了下幻想乡的历史、似乎层级和月都作战过、但最后以惨败结尾")
	CALL k1011GP("W", @"但有了这个可以轻易毁灭陨石的家伙存在、相比月都哪怕月球也不过如此吧")
	CALL k1011GPC("W", @"「毁灭月球?」", TARGET)
	CALL k1011GP("W", @"发出问题后、芙兰一脸疑惑的看向自己、接着拍了拍翅膀飞到自己面前、把小手贴到了自己的脑袋上")
	CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%是不是发烧了、为什么总喜欢问这么奇怪的问题」", TARGET)
	CALL k1011GP("W", @"在经过一段尴尬的沉默后、眼前的少女做出了回答")
	CALL k1011GPC("W", @"「毁灭月球确实很容易、但是为什么要毁灭月球呢?」", TARGET)
	CALL k1011GPC("W", @"「毁灭了月球、%k1011Name(TARGET, TARGET)%就没法赏月了啊」", TARGET)
	CALL k1011GPC("W", @"「月都?那是什么?月面战争又是什么?」", TARGET)
	CALL k1011GP("W", @"自己做出解释后、芙兰的大眼睛里又闪过了一丝天真好奇的光芒")
	CALL k1011GP("W", @"看来和一个天真可爱的少女谈论战争真是一个错误的选择")
	
;四重存在
@k1011ComMessageSpeakFourOfAKind()
	CALL k1011GP("W", @"分身是怎么样的一种体验呢?自己这种没分身的人是搞不明白了")
	CALL k1011GP("W", @"但是面前的少女可是有三个分身的究极生物、问问她应该能解决心中的疑惑吧")
	CALL k1011GPC("W", @"「呐、分身吗……其实就是%k1011Name(TARGET, TARGET)%分开的三个性格了」", TARGET)
	CALL k1011GPC("W", @"「她们都是有独立的意识到哦」", TARGET)
	CALL k1011GPC("W", @"「独立的意识?想维持这么多人时时刻刻意见保持统一怎么看都不像什么容易的事吧」", TARGET)
	CALL k1011GP("W", @"少女微微点了点头")
	CALL k1011GPC("W", @"「不过吵起架来、%k1011Name(TARGET, TARGET)%可以随时把她们收回去」", TARGET)
	CALL k1011GPC("W", @"「回去之后我们的思想什么的都会融为一体」", TARGET)
	CALL k1011GPC("W", @"「所以谁对谁错就不重要了」", TARGET)

;打则
@k1011ComMessageSpeakDaze()
	CALL k1011GPC("W", @"「非想天则?那是什么东西?」", TARGET)
	CALL k1011GP("W", @"看来芙兰还不知道这款风靡全球的格斗大作")
	IF K1011DEventFlow:车万游戏 > 0
		CALL k1011GP("W", @"但问题不大、稍加培养就可以代替自己去和别人pk了")
	ELSE
		CALL k1011GP("W", @"以这位少女打STG的水平来看、这位少女的潜力是不可估量的")
	ENDIF
	CALL k1011GP("W", @"为了能有新打手帮自己揭掉打则菜鸟的名号、%CALLNAME:MASTER%开始费劲心思的向一脸懵逼的芙兰宣传起了「非想天则」")
	
;便当
@k1011ComMessageSpeakBento()
	CALL k1011GPC("W", @"「便当?」", TARGET)
	IF k1011GetTalent("恋慕")
		CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%是饿了吗?」", TARGET)
		;拿手菜
		IF k1011GetAbl("烹饪技能") >= 4 && RAND:3 == 0
			CALL k1011GPC("W", @"「今天%k1011Name(TARGET, TARGET)%抢来了一只鸡」", TARGET)
			CALL k1011GPC("W", @"「等会给%k1011Name(TARGET, MASTER)%做%k1011Name(TARGET, TARGET)%的拿手菜」", TARGET)
			CALL k1011GPC("W", @"「碳烤蝾螈」", TARGET)
			CALL k1011GPC("W", @"「尽情期待吧」", TARGET)
		ELSEIF k1011GetExp("烹饪经验")
			CALL k1011GPC("W", @"「嗯、%k1011Name(TARGET, MASTER)%想吃什么啊?」", TARGET)
			CALL k1011GPC("W", @"「西红柿炒鸡蛋、还是草莓蛋糕?」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%会努力的」", TARGET)
		ELSE
			CALL k1011GPC("W", @"「嗯……%k1011Name(TARGET, TARGET)%似乎从来没做过饭啊……」", TARGET)
			CALL k1011GPC("W", @"「第一次做的话可能会有点难吃……」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%如果不嫌弃的话%k1011Name(TARGET, TARGET)%会试试的」", TARGET)
		ENDIF
	ELSE
		CALL k1011GPC("W", @"「是要给%k1011Name(TARGET, TARGET)%做便当吗?」", TARGET)
		CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%的胃口可是很特别的、要好好的记住了哦」", TARGET)
		CALL k1011GPC("W", @"「咳咳咳、%k1011Name(TARGET, TARGET)%喜欢甜的、一定要给%k1011Name(TARGET, TARGET)%做甜点」", TARGET)
		CALL k1011GPC("W", @"「不是甜点也可以、做%k1011Name(TARGET, TARGET)%家乡的食物也可以、但不要那奇奇怪怪的和食」", TARGET)
		CALL k1011GPC("W", @"「垃圾食物也可以……不过说回来明明这么好吃、为什么被冠以垃圾的胃口呢?」", TARGET)
		CALL k1011GP("W", @"面前的少女爹爹不休的演讲着自己的食物偏好、这么多要求、还真是让人头大啊")
		CALL k1011GPC("W", @"「好了、快去给%k1011Name(TARGET, TARGET)%做便当吧」", TARGET)
	ENDIF

;一天洗几次澡
@k1011ComMessageSpeakBathe()
	CALL k1011GPC("W", @"「一天洗几次澡?」", TARGET)
	IF k1011GetAbl("受虐感") >= 3 && k1011GetTalent("恋慕")
		CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%会陪%k1011Name(TARGET, TARGET)%一起去吗?」", TARGET)
		CALL k1011GPC("W", @"「想想%k1011Name(TARGET, MASTER)%把流水扑到%k1011Name(TARGET, TARGET)%身上……那股刺骨的疼痛感♪」", TARGET)
		CALL k1011GPC("W", @"「太棒了♥想想都很幸福♥」", TARGET)
		CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%快和%k1011Name(TARGET, TARGET)%一起去洗澡吧♥」", TARGET)
		CALL k1011GP("W", @"抖M吸血鬼少女似乎因自己的问题兴奋了起来\n面部渐渐泛起了潮红、一缕似有似为的爱心从红宝石般的大眼睛里划过")
	ELSE
		CALL k1011GPC("W", @"「让吸血鬼去洗澡?%k1011Name(TARGET, MASTER)%是想谋财害命的吗?」", TARGET)
		CALL k1011GPC("W", @"「还好%k1011Name(TARGET, TARGET)%是高贵的恶魔之妹、流水的影响并没有那么大、但被流水冲刷着也会很疼」", TARGET)
		CALL k1011GPC("W", @"「所以洗澡这一辈子都不可能的」", TARGET)
		CALL k1011GP("W", @"面前的少女怎么看都像精致的洋娃娃、周围都飘着幼女独有的清香、怎么看都不像不洗澡的样子")
		CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%可真是笨蛋啊」", TARGET)
		CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%可是魔法少女、清洁魔法效果不比搓澡好?」", TARGET)
	ENDIF

;最喜欢的玩具
@k1011ComMessageSpeakToy()
	CALL k1011GPC("W", @"「最喜欢的玩具?」", TARGET)
	CALL k1011GPC("W", @"「嗯……是小熊先生捏」", TARGET)
	CALL k1011GPC("W", @"「在没有%k1011Name(TARGET, MASTER)%的时候只有小熊先生陪伴%k1011Name(TARGET, TARGET)%」", TARGET)
	CALL k1011GP("W", @"芙兰一边说着、一边伸出手掌、伴随着手心划过一轮繁杂的魔法阵、一道通向玩具箱的传送门被撕开了")
	CALL k1011GP("W", @"一只看似纽扣耷拉着、打满补丁的玩具熊被抓了出来")
	;坏掉的差分
	SIF K1011Future == 0
		CALL k1011GP("W", @"在少女猛地抓取下、玩具熊承受不住这么大力、「嘣」的断了几根线、露出了如同白骨般雪白的棉花")
	CALL k1011GPC("W", @"「当当当、这就是小熊先生」", TARGET)
	IF k1011GetTalent("恋慕")
		CALL k1011GPC("W", @"「小熊先生已经这么破了啊」", TARGET)
		;坏掉的差分
		IF K1011Future == 0
			CALL k1011GPC("W", @"「但是问题不大、%k1011Name(TARGET, TARGET)%已经找到了新的玩具——%k1011Name(TARGET, MASTER)%」", TARGET)
			CALL k1011GPC("W", @"「即时没了小熊先生%k1011Name(TARGET, MASTER)%还会继续陪%k1011Name(TARGET, TARGET)%玩的吧?」", TARGET)
			CALL k1011GP("W", @"面前的少女抬起头、用看不到一丝光明的猩红大眼睛、像是抓到新猎物般看着自己")
