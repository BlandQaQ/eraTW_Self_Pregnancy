@k1011FirstOver
	;-------------------------------------------------
	;初次润滑超过LV2
	;-------------------------------------------------
	IF k1011GetNowPalam("润滑") > PALAMLV:2 && !K1011TrainArriveFirst:润滑
		;恋慕
		IF k1011GetTalent("恋慕")
			;使用润滑液
			IF k1011GetNowSelectcom() == 60
				CALL k1011GPC("W", @"「什麼?这个、滑溜溜的……用这个做什麼?」", TARGET)
			;除此之外
			ELSE
				CALL k1011GPC("W", @"「啊?下面已经湿了哦♪」", TARGET)
			ENDIF
		;除此之外
		ELSE
			;使用润滑液
			IF k1011GetNowSelectcom() == 60
				CALL k1011GPC("W", @"「什麼?这个、滑溜溜的……用这个做什麼?」", TARGET)
			;除此之外
			ELSE
				WAIT
				IF K1011InfOfSex == 1
					CALL k1011GP("W", @"芙兰的阴唇開始流出爱液、芙兰见状惊呼到")
					CALL k1011GPC("W", @"「什麼?这是什麼啊！?」", TARGET)
					CALL k1011GP("L", @"芙兰以为秘部出来的只是尿、露出困惑的表情")
					CALL k1011GP("W", @"告诉他这是很自然的事情、没什麼好奇怪的")
					CALL k1011GPC("W", @"「哦、原来从小穴里流出爱液、是让人舒服的事啊……%k1011Name(TARGET, MASTER)%的知识好多啊」", TARGET)
				ELSE
					CALL k1011GP("W", @"爱液从芙兰的阴唇渗出")
					CALL k1011GP("W", @"%CALLNAME:MASTER%故意发出嘟嘟嘟嘟的声音来调弄芙兰、搞得她满脸通红")
					CALL k1011GPC("W", @"「不要啊…不要那麼玩弄啊…%k1011Name(TARGET, MASTER)%♥」", TARGET)
				ENDIF
			ENDIF
		ENDIF
		K1011TrainArriveFirst:润滑 = 1
	ENDIF

	;-------------------------------------------------
	;初次欲情超过LV3
	;-------------------------------------------------
	IF k1011GetNowPalam("欲情") > PALAMLV:3 && !K1011TrainArriveFirst:欲情
		;恋慕
		IF k1011GetTalent("恋慕")
			;使用媚药
			IF k1011GetNowSelectcom() == 61
				CALL k1011GPC("W", @"「啊……这药是什麼药……怎麼说呢、好想赶紧做啊……」", TARGET)
			;除此之外
			ELSE
				CALL k1011GPC("W", @"「啊、%k1011Name(TARGET, TARGET)%……好像有点想做h事情了……」", TARGET)
			ENDIF
		;除此之外
		ELSE
			;使用媚药
			IF k1011GetNowSelectcom() == 61
				CALL k1011GPC("W", @"「啊……这药是什麼药……怎麼说呢、好想赶紧做啊……」", TARGET)
			;除此之外
			ELSE
				CALL k1011GPC("W", @"「啊、%k1011Name(TARGET, TARGET)%……好像有点想做h事情了……」", TARGET)
			ENDIF
		ENDIF
		K1011TrainArriveFirst:欲情 = 1
	ENDIF

	;-------------------------------------------------
	;初次耻情超过LV2
	;-------------------------------------------------
	IF k1011GetNowPalam("耻情") > PALAMLV:2 && !K1011TrainArriveFirst:耻情
		;恋慕
		IF k1011GetTalent("恋慕")
			CALL k1011GP("W", @"芙兰似乎因为%k1011Name(TARGET, MASTER)%的举动而感到羞耻")
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%、能不能……不要做太奇怪的事啊」", TARGET)
		;除此之外
		ELSE
			CALL k1011GP("W", @"芙兰似乎因为%k1011Name(TARGET, MASTER)%的举动而感到羞耻")
			CALL k1011GPC("W", @"「能不能……不要做太奇怪的事啊」", TARGET)
		ENDIF
		K1011TrainArriveFirst:耻情 = 1
	ENDIF

	;-------------------------------------------------
	;初次恐怖超过LV2
	;-------------------------------------------------
	IF k1011GetNowPalam("恐怖") > PALAMLV:2 && !K1011TrainArriveFirst:恐怖
		CALL k1011GP("W", @"芙兰对%CALLNAME:MASTER%感到恐惧")
		CALL k1011GPC("W", @"「这是什麼……害怕?第一次有这种感觉……」", TARGET)
		K1011TrainArriveFirst:恐怖 = 1
	ENDIF

	;-------------------------------------------------
	;初次C絶頂
	;-------------------------------------------------
	IF k1011GetNowex("Ｃ绝顶") && !K1011TrainArriveFirst:C绝顶
		CALL k1011GPC("L", @"「什麼东西来了…%k1011Name(TARGET, TARGET)%变得好奇怪!」", TARGET)
		CALL k1011GP("W", @"芙兰閉上眼睛抱住了%CALLNAME:PLAYER%、可能是害怕未知的海浪吧")
		CALL k1011GPC("W", @"「%k1011Sound("Orgasm", "呜、呜、啊啊——! !")%」", TARGET)
		CALL k1011GP("W", @"哔、哔、那小小的身体里、正翻涌着快乐的波浪")
		CALL k1011GP("W", @"芙兰第一次感受到了高潮、一边微微痉挛、一边盯着%CALLNAME:PLAYER%")
		CALL k1011GPC("W", @"「这是什麼啊……%k1011Name(TARGET, TARGET)%不知道啊……」", TARGET)
		CALL k1011GPC("W", @"「你说是去吗?嗯……%k1011Name(TARGET, TARGET)%、去了……」", TARGET)
		CALL k1011GP("W", @"芙兰羞红了脸、轻轻点了点头")
		K1011TrainArriveFirst:C绝顶 = 1
	ENDIF

	;-------------------------------------------------
	;初次V絶頂
	;-------------------------------------------------
	IF k1011GetNowex("Ｖ绝顶") && !K1011TrainArriveFirst:V绝顶
		CALL k1011GPC("W", @"「%k1011Sound("Orgasm", "啊!哈啊!")%」", TARGET)
		CALL k1011GP("W", @"芙兰的样子很奇怪、看来女人的高潮要来了")
		CALL k1011GPC("W", @"「小穴里的东西、太可怕了!」", TARGET)
		CALL k1011GPC("W", @"「大脑变成一片空白的了!%k1011Name(TARGET, MASTER)%！！」", TARGET)
		CALL k1011GP("W", @"在%CALLNAME:PLAYER%的臂弯里、小小的身体可爱地颤抖着、迎来了第一次的高潮")
		CALL k1011GPC("W", @"「%k1011Sound("Orgasm", "哈哈哈……")%%k1011Name(TARGET, MASTER)%♥%k1011Name(TARGET, TARGET)%…的小穴去的…♪」", TARGET)
		K1011TrainArriveFirst:V绝顶 = 1
	ENDIF

	;-------------------------------------------------
	;初次A絶頂
	;-------------------------------------------------
	IF k1011GetNowex("Ａ绝顶") && !K1011TrainArriveFirst:A绝顶
		CALL k1011GPC("W", @"「%k1011Sound("Orgasm", "啊、啊、")%%k1011Name(TARGET, TARGET)%的屁股好厉害啊!要变的奇怪了%k1011Sound("Orgasm", "啊!啊啊啊!")%」", TARGET)
		CALL k1011GP("W", @"从小就未被沾污的小身体、迎来了出生以来的第一次肛门高潮")
		CALL k1011GPC("W", @"「屁股……真的……坏……坏……了……」", TARGET)
		CALL k1011GP("W", @"坠入快乐的双眸呆呆地望着天空、嘴角淌着一缕口水")
		CALL k1011GP("W", @"幼小的肢体战战兢兢地反復痉挛、表现出强烈的快乐")
		K1011TrainArriveFirst:A绝顶 = 1
	ENDIF

	;-------------------------------------------------
	;初次B絶頂
	;-------------------------------------------------
	IF k1011GetNowex("Ｂ绝顶") && !K1011TrainArriveFirst:B绝顶
		CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%的胸部好奇怪、%k1011Sound("Orgasm", "啊啊!")%」", TARGET)
		CALL k1011GP("W", @"芙兰轻触自己痉挛的胸部、被自己的身体吓了一跳")
		CALL k1011GPC("W", @"「%k1011Sound("Orgasm", "哈……哈……")%乳房在颤抖呢……呀……」", TARGET)
		K1011TrainArriveFirst:B绝顶 = 1
	ENDIF

	;-------------------------------------------------
	;奉仕100/200
	;-----------------------------------------------
	IF k1011GetExp("侍奉快乐经验") > 100 && K1011TrainArriveFirst:侍奉快乐 == 0
		CALL k1011GP("W", @"芙兰開始体会到奉献的喜悦、很高兴的继续着行动。")
		CALL k1011GPC("W", @"「所谓的h、不只是一个人、而是两个人都能心情舒畅…」", TARGET)
		CALL k1011GPC("W", @"「呵呵。哎…做更多的色情事吧、一起心情变好吧♪」", TARGET)
		K1011TrainArriveFirst:侍奉快乐 = 1
	ELSEIF k1011GetExp("侍奉快乐经验") > 200 && K1011TrainArriveFirst:侍奉快乐 == 1
		CALL k1011GPC("W", @"「喂、舒服吗?」", TARGET)
		CALL k1011GPC("W", @"「呵呵……%k1011Name(TARGET, TARGET)%也很舒服哦…♪」", TARGET)
		CALL k1011GP("W", @"芙兰学会了为他人服务的快乐、行为中的眼神发生了变化")
		K1011TrainArriveFirst:侍奉快乐 = 2
	ENDIF
