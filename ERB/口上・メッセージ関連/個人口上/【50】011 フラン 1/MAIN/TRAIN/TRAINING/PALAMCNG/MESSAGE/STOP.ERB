;-------------------------------------------------
;寸止
;-------------------------------------------------
@k1011OrgasmStop
	;-------------------------------------------------
	;寸止失败(你先生不小心)
	;-------------------------------------------------
	IF k1011GetNowex("寸止失败(你先生)")
		;Ｖ绝顶高潮前停止失败
		IF NOWEX:1 > 0
			CALL k1011GPC("L", @"「%k1011Sound("Orgasm", "啊!啊啊!")%」", TARGET)
			CALL k1011GP("W", @"但是多刺激了一下、芙兰还是高潮了")
			CALL k1011GPC("W", @"「%k1011Sound("Orgasm", "啊啊啊啊啊啊! !")%%k1011Name(TARGET, TARGET)%喜欢你!♪」", TARGET)
		;Ｃ绝顶高潮前停止失败
		ELSEIF NOWEX:0 > 0
			CALL k1011GPC("L", @"「%k1011Sound("Orgasm", "啊……啊啊! !")%」", TARGET)
			CALL k1011GP("W", @"芙兰没有想到%CALLNAME:PLAYER%会这麼激烈、一边吃惊一边达到了高潮")
		;Ａ绝顶高潮前停止失败
		ELSEIF NOWEX:2 > 0
			CALL k1011GPC("L", @"「嗯、屁股、%k1011Sound("Orgasm", "哈哇哇!")%」", TARGET)
			CALL k1011GP("W", @"芙兰很兴奋地用肛门高潮了")
			CALL k1011GPC("W", @"「哇! !好舒服!」", TARGET)
		;Ｂ绝顶高潮前停止失败
		ELSEIF NOWEX:3 > 0
			CALL k1011GPC("L", @"「%k1011Sound("Orgasm", "啊……啊啊! !")%」", TARGET)
			CALL k1011GP("W", @"芙兰没有想到%CALLNAME:PLAYER%会这麼激烈、没做好准备的小身躯颤颤抖抖的达到了高潮")
		ENDIF
	;-------------------------------------------------
	;高潮前停止失败(芙兰酱忍不住了)
	;-------------------------------------------------
	ELSEIF k1011GetNowex("寸止失败(芙兰)")
		;Ｖ绝顶高潮前停止失败
		IF NOWEX:1 > 0
			;恋慕
			;性交系
			IF GROUPMATCH(k1011GetNowSelectcom(), 20, 21, 22, 23, 27, 28, 610, 612, 614, 615)
				IF RAND:2 == 0
					CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%是一个好色的孩子、忍不住了……!」", TARGET)
					CALL k1011GP("W", @"芙兰突然重新開始使用自己的腰、仿佛在用%CALLNAME:PLAYER%的阴茎自慰、但也达到了高潮")
					CALL k1011GPC("W", @"「%k1011Sound("Orgasm", "啊!啊!")%好舒服啊! !♪」", TARGET)
				ELSE
					CALL k1011GPC("W", @"「不行了!%k1011Name(TARGET, TARGET)%受不了又硬又大的肉棒了!」", TARGET)
					CALL k1011GPC("W", @"「%k1011Sound("Orgasm", "噫噫啊、恩!哈!")%%k1011Name(TARGET, PLAYER)%！！！」", TARGET)
				ENDIF
			;除此之外
			ELSE
				CALL k1011GPC("L", @"「%k1011Name(TARGET, TARGET)%忍不下去了!」", TARGET)
			ENDIF
		;Ｃ绝顶高潮前停止失败
		ELSEIF NOWEX:0 > 0
			CALL k1011GPC("L", @"「哦、豆子好难受!」", TARGET)
			CALL k1011GPC("W", @"「让%k1011Name(TARGET, TARGET)%去啊!%k1011Name(TARGET, TARGET)%受不了了!」", TARGET)
		;Ａ绝顶高潮前停止失败
		ELSEIF NOWEX:2 > 0
			CALL k1011GPC("W", @"「屁股再多刺激下啊! !」", TARGET)
			CALL k1011GPC("W", @"「去了!%k1011Name(TARGET, TARGET)%屁股要去了! !♪♪」", TARGET)
		;Ｂ绝顶高潮前停止失败
		ELSEIF NOWEX:3 > 0
			CALL k1011GPC("W", @"「这样的乳房、%k1011Name(TARGET, TARGET)%已经…受不了了!」", TARGET)
		ENDIF
	;-------------------------------------------------
	;寸止
	;-------------------------------------------------
	ELSEIF (k1011GetNowex("Ｃ高潮前停止累计数") || k1011GetNowex("Ｖ高潮前停止累计数") || k1011GetNowex("Ａ高潮前停止累计数") || k1011GetNowex("Ｂ高潮前停止累计数")) && k1011GetTequip("挑逗play")
		;Ｖ绝顶寸止
		IF NOWEX:21
			IF RAND:3 == 0
				CALL k1011GPC("W", @"「不要!肉棒不要停啊!」", TARGET)
				CALL k1011GP("W", @"芙兰面红耳赤地扭动着腰想要达到高潮、但%CALLNAME:PLAYER%用力按住了芙兰")
			ELSEIF RAND:2 == 0
				CALL k1011GP("W", @"%CALLNAME:PLAYER%感觉到芙兰的高潮、立刻停止了撞击")
				CALL k1011GPC("W", @"「哎呀、还差一点儿、还差一点儿就可以了、%k1011Name(TARGET, PLAYER)%♥……」", TARGET)
			ELSE
				CALL k1011GPC("W", @"「%k1011Name(TARGET, PLAYER)%……%k1011Name(TARGET, PLAYER)%♥……%k1011Name(TARGET, TARGET)%还没去呢…」", TARGET)
				CALL k1011GPC("W", @"「为什麼不动了?太狡猾了」", TARGET)
			ENDIF
		;Ｃ绝顶寸止
		ELSEIF NOWEX:20
			CALL k1011GPC("L", @"「不要停啊、不要停啊、这样好难受啊」", TARGET)
			CALL k1011GPC("W", @"「喂、可以再摸下吧?喂、喂……」", TARGET)
		;Ａ绝顶寸止
		ELSEIF NOWEX:22
			CALL k1011GPC("L", @"「屁股痒痒的……啊」", TARGET)
			CALL k1011GPC("W", @"「刚才还没去啊、再碰一下呗」", TARGET)
		;Ｂ绝顶寸止
		ELSEIF NOWEX:23
			CALL k1011GPC("L", @"「哦……噢……来了!」", TARGET)
			CALL k1011GPC("W", @"「嗯、差点儿就来了……」", TARGET)
		ENDIF
	ENDIF

	;-------------------------------------------------
	;喷乳前停止
	;-------------------------------------------------
	IF k1011GetNowex("喷乳前停止累计数")
			;寸止4次以上
		IF k1011GetEx("喷乳前停止累计数") >= 4
			CALL k1011GPC("L", @"「%k1011Name(TARGET, TARGET)%的乳房好涨、里面的奶越来越多了…」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, PLAYER)%、只要扑通一声、%k1011Name(TARGET, TARGET)%的乳房就能喷出奶哦」", TARGET)
			;除此之外
		ELSE
			CALL k1011GP("W", @"乳汁从不停地颤抖着的勃起的可爱乳头上滴下")
			CALL k1011GPC("L", @"「啊、刚才好像要从乳房里出来了……再做一次呗、再做一次呗」", TARGET)
		ENDIF
	ENDIF

	;-------------------------------------------------
	;空射精
	;-------------------------------------------------
	IF k1011GetNowex("空射精累计数") && k1011GetTequip("禁止射精")
		;空射精累计次数差分
		IF k1011GetEx("空射精累计数") >= 15
			CALL k1011GPC("L", @"「呜!呜呜呜!%k1011Name(TARGET, TARGET)%射不了精了!%k1011Name(TARGET, TARGET)%射不了精了! !」", TARGET)
			CALL k1011GPC("W", @"「嗨、嗨!快拿掉!%k1011Name(TARGET, PLAYER)%！」", TARGET)
		ELSEIF k1011GetEx("空射精累计数") >= 5
			CALL k1011GP("W", @"红黑色膨胀的阴茎持续痉挛着、芙兰热泪盈眶")
			CALL k1011GPC("L", @"「不行了……不行了……%k1011Name(TARGET, TARGET)%的肉棒要坏掉了……」", TARGET)
		;除此之外
		ELSE
			IF RAND:2 == 0
				CALL k1011GPC("W", @"「太奇怪了……太奇怪了」", TARGET)
				CALL k1011GPC("W", @"「刚才应该从%k1011Name(TARGET, TARGET)%的肉棒里喷出来了、可是没出来啊……」", TARGET)
			ELSE
				CALL k1011GPC("L", @"「肉棒……呜呜、不能射精了、哟!」", TARGET)
				CALL k1011GPC("W", @"「%k1011Name(TARGET, PLAYER)%♥……可以把这个拿掉吧?喂、可以的吧?」", TARGET)
			ENDIF
		ENDIF
	ENDIF

	;-------------------------------------------------
	;射精渇望中
	;-------------------------------------------------
	IF k1011GetNowex("射精前停止累计数")
		;寸止4次差分
		IF k1011GetEx("射精前停止累计数") >= 4
			CALL k1011GPC("L", @"「啊、呜呜呜!」", TARGET)
			CALL k1011GPC("W", @"「……啊、又来了?」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, PLAYER)%♥求你了、让%k1011Name(TARGET, TARGET)%的肉棒哔一声吧!」", TARGET)
		;除此之外
		ELSE
			CALL k1011GP("W", @"芙兰那可爱的阴茎贪得无厌地抖动着")
			CALL k1011GPC("L", @"「啊……%k1011Name(TARGET, TARGET)%还以为肉棒哔了呢」", TARGET)
		ENDIF
	ENDIF
	IF k1011GetBase("射精") == k1011GetMaxbase("射精") - 1
		;ntr
		IF (k1011GetNowSelectcom() == 107 && k1011GetTequip("捆绑") && k1011GetTequip("野外play")) || GROUPMATCH(k1011GetNowSelectcom(), 105, 106)
			CALL k1011NtrKiller()
		ELSE
			IF RAND:4 == 0
				CALL k1011GP("L", @"芙兰即将达到高潮的阴茎随着呼吸在微微颤抖")
				CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%的肉棒、差一点就出来了哦…再刺激一下呗…」", TARGET)
			ELSE
			ENDIF
		ENDIF
	ENDIF

	;-------------------------------------------------
	;放尿渇望
	;-------------------------------------------------
	IF k1011GetNowex("放尿前停止累计数")
			;寸止め4回分以上
		IF k1011GetEx("放尿前停止累计数") >= 4
			CALL k1011GPC("W", @"「不行了!让%k1011Name(TARGET, TARGET)%当变态也行啊、好想在这里尿!」", TARGET)
			CALL k1011GPC("W", @"「不然膀胱就坏了!%k1011Name(TARGET, PLAYER)%！！」", TARGET)
		ELSE
			CALL k1011GPC("W", @"「啊、啊……尿出来了——!」", TARGET)
			CALL k1011GPC("L", @"「咦?…呀、%k1011Name(TARGET, PLAYER)%♥……为什麼要停啊……」", TARGET)
			CALL k1011GP("W", @"芙兰察觉到自己说的令人羞愧的话、脸涨得通红、把目光从%CALLNAME:PLAYER%身上移開")
		ENDIF
	ENDIF
	IF k1011GetBase("尿意") == k1011GetMaxbase("尿意") - 1
		;ntr
		IF (k1011GetNowSelectcom() == 107 && k1011GetTequip("捆绑") && k1011GetTequip("野外play")) || GROUPMATCH(k1011GetNowSelectcom(), 105, 106)
			CALL k1011NtrKiller()
		ELSE
			IF RAND:4 == 0
				CALL k1011GP("W", @"芙兰双手按在胯下、不停地摇晃着身体、忍受着尿意")
				CALL k1011GPC("L", @"「哎呀……不行了、尿要漏出来了……」", TARGET)
			ELSEIF RAND:3 == 0
				CALL k1011GP("W", @"芙兰满脸通红、閉着眼睛微微颤抖着、好像在忍耐尿意")
				CALL k1011GPC("L", @"「不行、要出了、要出了啊……」", TARGET)
			ELSE
			ENDIF
		ENDIF
	ENDIF
