;-------------------------------------------------
;破处
;-------------------------------------------------
@k1011LoseVirgin
	IF k1011GetOtherInfo("处女丧失")
		;恋慕因你先生丧失处女
		IF k1011GetOtherInfo("因为你先生丧失处女")
			;无反发
			IF k1011GetTalent("恋慕") && !k1011GetMark("反发刻印")
				CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%……」", TARGET)
				CALL k1011GP("W", @"芙兰紧紧地与%CALLNAME:MASTER%抱在一起、更加深情地索求接吻")
				CALL k1011GPC("L", @"「那个、%k1011Name(TARGET, TARGET)%早都很想做…但不知该怎麼做……那个、%k1011Name(TARGET, MASTER)%、%k1011Name(TARGET, TARGET)%很高兴能这样做」", TARGET)
				CALL k1011GPC("W", @"「一直住在房间里、很无聊、能见都%k1011Name(TARGET, MASTER)%……太好了」", TARGET)
			;有反发
			ELSE
				CALL k1011GPC("W", @"「呼、呼……%k1011Name(TARGET, MASTER)%、%k1011Name(TARGET, TARGET)%还以为你会对我好点呢……」", TARGET)
				CALL k1011GPC("W", @"「但因为是%k1011Name(TARGET, MASTER)%……%k1011Name(TARGET, TARGET)%并不讨厌……」", TARGET)
			ENDIF
			K1011GiveMasterVirgin = 1
		;再生处女
		ELSEIF K1011VirginKind == 2
			CALL k1011GPC("W", @"「%k1011Sound("Happy", "啊、哈……哼……")%」", TARGET)
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%再一次拿走了%k1011Name(TARGET, TARGET)%的第一次♪」", TARGET)
			CALL k1011GPC("W", @"「虽然不是第一次、但能让%k1011Name(TARGET, MASTER)%夺走%k1011Name(TARGET, TARGET)%很高兴…♪」", TARGET)
		;手指插入导致处女喪失
		ELSEIF k1011GetNowSelectcom() == 4
			;恋慕
			IF k1011GetTalent("恋慕") == 1
				CALL k1011GP("W", @"%CALLNAME:PLAYER%把手指拔出来、不出所料、手指被染成了红色")
				CALL k1011GP("W", @"里面相当紧、%CALLNAME:MASTER%正想用手拿走少女的处女是否不太好、胳膊就被人抓走了")
				CALL k1011GPC("W", @"「%k1011Sound("Mouth", "啾……啾……")%」", TARGET)
				CALL k1011GP("W", @"芙兰专心致志地舔舐着自己的纯洁、结束后、撒娇似的抱住了%CALLNAME:MASTER%")
				CALL k1011GPC("W", @"「喂、接下来要做什麼?一起做快乐的事情吧~ ~」", TARGET)
				CALL k1011GP("W", @"只是觉得芙兰的表情和举动有点奇怪")
			;除此之外
			ELSE
				CALL k1011GPC("W", @"「有点疼……你刚才做了什麼?」", TARGET)
				CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%长大了?嗯……」", TARGET)
				CALL k1011GP("W", @"芙兰没有意识到事情的严重性、只是看着自己纯洁的证据感到不可思议")
			ENDIF
		;除此之外の時に处女喪失
		ELSE
			IF ASSIPLAY && ASSI > 0 
				CALL k1011GPC("W", @"「哇……也不是很痛吗……」", TARGET)
				CALL k1011GPC("W", @"「洞里面好奇怪啊……」", TARGET)
				CALL k1011GPC("W", @"「好像有东西流出来了、会火辣辣的……」", TARGET)
			;恋慕
			ELSEIF k1011GetTalent("恋慕") == 1
				CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%、%k1011Name(TARGET, TARGET)%……」", TARGET)
				CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%的第一次要是能给%k1011Name(TARGET, MASTER)%……这样就好了……」", TARGET)
				CALL k1011GPC("W", @"「呜、呜……哇啊啊! !」", TARGET)
				CALL k1011GP("W", @"芙兰很像个少女、放声大哭起来")
				CALL k1011GP("W", @"从那被蹂躏的小伤口上垂落着被撕碎的纯洁的证据")
			;除此之外
			ELSE
				CALL k1011GPC("W", @"「哇……也不是很痛吗……」", TARGET)
				CALL k1011GPC("W", @"「洞里面好奇怪啊……」", TARGET)
				CALL k1011GPC("W", @"「好像有东西流出来了、会火辣辣的……」", TARGET)
			ENDIF
		ENDIF
	ENDIF
