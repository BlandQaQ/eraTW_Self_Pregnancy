		CALL k1011GP("W", @"随着两人的嘴唇分開、一条晶莹透亮的丝线反射着月光")
		CALL k1011GP("W", @"随后、芙兰酱脱下了衣服……")
		CALL k1011GalSetMood("Love")
		CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%♥……要更加的疼爱%k1011Name(TARGET, TARGET)%哦♪」", TARGET)
		CALL k1011GalSetMood("")
		K1011FlanCallMaster = 16
	ELSEIF RESULT == 1
		CALL k1011GalSetMood("Sing")
		CALL k1011GPC("W", @"「嗯、%k1011Name(TARGET, TARGET)%知道了、%k1011Name(TARGET, MASTER)%♪」", TARGET)
		CALL k1011GP("W", @"随着两人的嘴唇分開、一条晶莹透亮的丝线反射着月光。")
		CALL k1011GP("W", @"随后、芙兰酱脱下了衣服……")
		CALL k1011GPC("W", @"「嘻嘻、開始做吧♪」", TARGET)
		CALL k1011GalSetMood("")
	ENDIF
	PRINTL 　
	CALL k1011GP("W", @"『今后、可以在「口上设置」里更改称呼』")
