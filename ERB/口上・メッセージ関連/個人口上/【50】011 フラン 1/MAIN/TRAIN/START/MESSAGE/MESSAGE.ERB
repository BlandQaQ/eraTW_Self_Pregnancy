		CALL k1011GPC("W", @"「但是喜欢是这种时候使用的语言吗……？」", TARGET)
		CALL k1011GalSetMood("")
		CALL k1011GPC("L", @"「但是、对%k1011Name(TARGET, MASTER)%说出来的时候、那些话就变成了『喜欢』」", TARGET)
		CALL k1011GalSetMood("Love")
		CALL k1011GPC("W", @"「喜欢、喜欢呦……」", TARGET)
		CALL k1011GPC("W", @"「唔……这种话说出来了……总觉得、特别的幸福」", TARGET)
		CALL k1011GP("W", @"芙兰浅浅的笑著、红色的眼睛中突然落下了泪滴")
		CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%、从一出生就是一个人…这样天天陪%k1011Name(TARGET, TARGET)%的人、只有%k1011Name(TARGET, MASTER)%一个人…」", TARGET)
		CALL k1011GPC("W", @"「和%k1011Name(TARGET, MASTER)%有更多接触什麼的、芙兰有点想要呢……」", TARGET)
		CALL k1011GalSetMood("")
		CALL k1011GP("W", @"…%CALLNAME:MASTER%把芙兰的帽子取了下来、把手放在了柔软的金发上、温柔的抚摸著")
		CALL k1011GP("W", @"芙兰瞇起眼睛…%CALLNAME:MASTER%与那小小的嘴唇重合在了一起")
		CALL k1011GSleep
		CALL k1011GPC("W", @"「那个…有一件事可以打听一下吗？」", TARGET)
	ELSE
		CALL k1011GP("W", @"刚来到芙兰的房间前……就听见里面传来流畅的英文的歌声")
		CALL k1011GalSetMood("Sing")
		CALL k1011GPC("W", @"「～～♪ ～♪」", TARGET)
		CALL k1011GalSetMood("")
		CALL k1011GP("W", @"那毫无疑问是芙兰充满爱意的歌声……")
		CALL k1011GP("W", @"旋律听起来有些悲伤")
		CALL k1011GalSetMood("Suprise")
		CALL k1011GPC("W", @"「啊…」", TARGET)
		CALL k1011GalSetMood("")
		CALL k1011GP("W", @"%CALLNAME:MASTER%进入了房间、芙兰有些不安的脸蛋靠近了一些")
		CALL k1011GPC("W", @"「……那个啊、%k1011Name(TARGET, TARGET)%呢、一直都待在房间里、没有去过外面」", TARGET)
		CALL k1011GPC("W", @"「一直都是一个人玩」", TARGET)
		CALL k1011GPC("W", @"「但是、但是呢」", TARGET)
		CALL k1011GP("W", @"芙兰把%CALLNAME:MASTER%紧紧地抱住、抬头向上看")
		CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%已经不想继续一个人玩了」", TARGET)
		CALL k1011GPC("W", @"「即使不能到外面也好、疼痛什麼的也可以忍耐、只要……」", TARGET)
		CALL k1011GPC("W", @"「能和%k1011Name(TARGET, MASTER)%在一起、陪伴在%k1011Name(TARGET, MASTER)%的身边！」", TARGET)
		CALL k1011GPC("W", @"「………拜托了……真的希望能够这样……」", TARGET)
		CALL k1011GP("W", @"%CALLNAME:MASTER%紧紧地抱住了哭了起来的芙兰、抚摸著她的小脑袋")
		CALL k1011GPC("W", @"「十分…感谢……%k1011Name(TARGET, MASTER)%……呜呜呜呜呜呜……」", TARGET)
		CALL k1011GalSetMood("Love")
		CALL k1011GPC("W", @"「呜……那个……那个……喜欢%k1011Name(TARGET, MASTER)%、最喜欢%k1011Name(TARGET, MASTER)%了、好想、一直在一起」", TARGET)
		CALL k1011GalSetMood("")
		CALL k1011GP("W", @"在%CALLNAME:MASTER%的注视下、眼角珠泪芙兰还是露出了微笑")
		CALL k1011GSleep
		CALL k1011GP("W", @"暂时镇定了一些之后、芙兰看著%CALLNAME:MASTER%開了口")
		CALL k1011GPC("W", @"「……那个、有一件事……可以听%k1011Name(TARGET, TARGET)%说下吗？」", TARGET)
	ENDIF
	;猪脚性别差分
	IF k1011GetTalent("性别", MASTER) == 2
		CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%有一个姐姐大人、非常想要一个哥哥大人……」", TARGET)
		CALL k1011GPC("W", @"「所以说、从今以后、把%k1011Name(TARGET, MASTER)%称呼为哥哥、或者哥哥大人什麼的……可以吗？」", TARGET)
		CALL k1011GPC("W", @"「……或者说、就像现在这样、%k1011Name(TARGET, MASTER)%、的话比较好？」", TARGET)
		CALL k1011GPC("W", @"「…或者说、主人大人、什麼的…」", TARGET)
	ELSE
		CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%曾经有个姐姐大人、%k1011Name(TARGET, TARGET)%也希望%k1011Name(TARGET, MASTER)%也能做%k1011Name(TARGET, TARGET)%的姐姐……」", TARGET)
		CALL k1011GPC("W", @"「所以说、从今以后、能不能把%k1011Name(TARGET, MASTER)%称呼为姐姐…」", TARGET)
		CALL k1011GPC("W", @"「……或者像咲夜那样的小姐？」", TARGET)
		CALL k1011GPC("W", @"「或者……继续叫%k1011Name(TARGET, MASTER)%？」", TARGET)
	ENDIF
	PRINTL 　
	;称呼名变更
	CALL k1011SelectCallNameFlan()
	PRINTL 　
	DRAWLINE
	CALL k1011GP("W", @"『今后、可以在「芙兰口上设置」里更改称呼』")

;亲爱取得口上
@k1011MeetGetLove2()
	CALL k1011GalSetMood("Sing")
	CALL k1011GPC("L", @"「～～♪ ～♪」", TARGET)
	CALL k1011GalSetMood("")
	CALL k1011GP("W", @"当进入房间时、芙兰酱一边哼着歌、一边悠闲的晃着身体")
	CALL k1011GPC("W", @"「～～♪%k1011Name(TARGET, MASTER)%」", TARGET)
	CALL k1011GP("L", @"%CALLNAME:MASTER%以为走到旁边时、芙兰会像以往一样天真地拥抱自己、但结果并非如此")
	CALL k1011GP("W", @"芙兰的表情却极为平静、那双疯狂的双眼也平静了下来")
	CALL k1011GP("W", @"当%CALLNAME:MASTER%坐在芙兰酱身旁是、芙兰酱轻轻地靠在了%CALLNAME:MASTER%身上")
	CALL k1011GPC("W", @"「……那个、很久……很久以前」", TARGET)
	CALL k1011GPC("W", @"「有人告诉我、要寻找幸福」", TARGET)
	CALL k1011GPC("W", @"「寻找幸福时、会经历痛苦、也会经历流泪」", TARGET)
	CALL k1011GPC("W", @"「但是、保持開心快乐的心情一直是最重要的」", TARGET)
	CALL k1011GPC("W", @"「那是最难做到的事情」", TARGET)
	CALL k1011GPC("W", @"「我是这麼认为的...和姐姐,大家一起玩时\n总是抱着開心快乐的心情」", TARGET)
	CALL k1011GPC("W", @"「所以我总觉得十分快乐」", TARGET)
	CALL k1011GPC("W", @"「但是、刚開始和%k1011Name(TARGET, MASTER)%一起玩时、总是抱着痛苦悲伤的心情」", TARGET)
	CALL k1011GPC("W", @"「所以我才会感到不開心」", TARGET)
	CALL k1011GPC("W", @"「但带着開心快乐的心情回想……一回想起来、」", TARGET)
	CALL k1011GPC("W", @"「也感觉…、很開心、很快乐…」", TARGET)
	CALL k1011GPC("W", @"「就是这样的吧」", TARGET)
	CALL k1011GalSetMood("Love")
	CALL k1011GPC("W", @"「我……我的幸福就是...和%k1011Name(TARGET, MASTER)%在一起……」", TARGET)
	CALL k1011GalSetMood("")
	PRINTL
	CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%……能紧紧的抱着我吗？」", TARGET)
	CALL k1011GSleep
	CALL k1011GPC("W", @"「那个、%k1011Name(TARGET, MASTER)%……和你过了这麼久、还不知道你的生日是几号……」", TARGET)
	CALL k1011GPC("W", @"「可以告诉芙兰吗?」", TARGET)
	CALL k1011GP("SL", @"要告诉芙兰你的生日吗?")
	CALL k1011GSetQ("当然可以", "这是我的个人秘密")
	CALL k1011GInput()
	IF RESULT == 0
		CALL k1011AskBirthDayMain()
		IF K1011MasterBirthday:0 == 7 && K1011MasterBirthday:1 == 4
			CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%的生日也是7月4!?」", TARGET)
			CALL k1011GPC("W", @"「芙兰的生日也是7月4哦」", TARGET)
			CALL k1011GPC("W", @"「看来上天也希望我们俩能在一起啊♪」", TARGET)
		ENDIF
	ELSEIF RESULT == 1
		CALL k1011GPC("W", @"「不告诉芙兰也是可以的」", TARGET)
		CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%也是要有%k1011Name(TARGET, MASTER)%的秘密的」", TARGET)
	ENDIF

;相爱取得口上
@k1011MeetGetLove3()
	CALL k1011GalSetMood("Love")
	CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%?」", TARGET)
	CALL k1011GP("W", @"芙兰抱住了%CALLNAME:MASTER%、在%CALLNAME:MASTER%身上缓缓蹭着、用全身来表达自己爱意")
	CALL k1011GPC("W", @"「%k1011Name(TARGET, MASTER)%喜欢%k1011Name(TARGET, TARGET)%吧？」", TARGET)
	CALL k1011GP("W", @"%CALLNAME:MASTER%向芙兰酱点点头、她便露出了天真的笑容")
	CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%也最喜欢%k1011Name(TARGET, MASTER)%了」", TARGET)
	CALL k1011GPC("W", @"「所以、我们结婚吧♪」", TARGET)
	CALL k1011GP("W", @"我点了点头、芙兰害羞地把身体贴了过来")
	CALL k1011GPC("W", @"「要一直、一直在一起哦♪」", TARGET)
	CALL k1011GP("W", @"两个人嘴唇碰在了一起")
