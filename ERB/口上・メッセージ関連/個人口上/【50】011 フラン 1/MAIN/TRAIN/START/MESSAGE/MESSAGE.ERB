	CALL k1011GSetQ("那是当然的", "保持原来的称呼就行")
	CALL k1011GInput()
	IF RESULT == 0
		CALL k1011GalSetMood("Sing")
		CALL k1011GPC("W", @"「嗯、%k1011Name(TARGET, TARGET)%知道了、达令♪」", TARGET)
