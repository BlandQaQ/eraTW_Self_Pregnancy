;恋慕取得口上
@k1011MeetGetLove1()
	;性格分歧
	IF K1011Character >= 2
		CALL k1011GP("W", @"刚来到芙兰的房间前……就听见里面传来流畅的英文的歌声")
		CALL k1011GalSetMood("Sing")
		CALL k1011GPC("W", @"「～～♪ ～♪」", TARGET)
		CALL k1011GP("W", @"那毫无疑问是芙兰充满爱意的歌声……")
		CALL k1011GP("W", @"旋律听起来有些悲伤")
		CALL k1011GalSetMood("Suprise")
		CALL k1011GPC("W", @"「啊…」", TARGET)
		CALL k1011GalSetMood("")
		CALL k1011GP("W", @"门被打開的同时旋律随之停止、芙兰的脸上出现了複雜的表情")
		CALL k1011GPC("W", @"「呐…为什麼又来到这个房间了？」", TARGET)
		CALL k1011GPC("W", @"「为什麼又来和%k1011Name(TARGET, TARGET)%一起玩呢？」", TARGET)
		CALL k1011GPC("L", @"「我想对%k1011Name(TARGET, MASTER)%说出来的话明明有很多」", TARGET)
		CALL k1011GPC("W", @"「但是、那些话总是迅速的浮现出来、又迅速的消失了…」", TARGET)
		CALL k1011GPC("W", @"「现在什麼话都说不出来……感觉非常的难受……」", TARGET)
		CALL k1011GPC("W", @"「%k1011Name(TARGET, TARGET)%这是怎麼了？ 完全不明白…」", TARGET)
		CALL k1011GP("W", @"芙兰自己就像话里说的一样、看起来略有不安的脸微微倾斜著")
		CALL k1011GalSetMood("Love")
		CALL k1011GPC("L", @"「%k1011Name(TARGET, TARGET)%…喜欢%k1011Name(TARGET, MASTER)%…了呢……」", TARGET)
