;以ym的格式展示日期
@k1011YmMoneyShow(num)
	#FUNCTIONS
	#DIM num
	#DIMS out, 3
	SIF num >= 10000
		out:1 = {num / 10000}萬
	SIF LOCAL % 10000 > 0
		LOCALS:2 = {LOCAL % 10000}
	out '= out:1 + out:2
	RETURNF out



