;根据难度获得期限
@k1011GetDealine(difficult)
	#FUNCTION
	#DIM difficult
	SELECTCASE difficult
		;EASY(1周目のみ期限＋0日)
		CASE 1
			RETURNF 120
		;NORMAL
		CASE 2
			RETURNF 100
		;HARD
		CASE 3
			RETURNF 90
		;LUNATIC(
		CASE 4
			RETURNF 80
		;PHANTASM
		CASE 5
			RETURNF 70
		;ULTRA
		CASE 6
			RETURNF 50
		;OVERDRIVE
		CASE 7
			RETURNF 40
		;UNKOWN
		CASEELSE
			RETURNF 100
	ENDSELECT

;醉酒情况,从76哪里cp的
;0没醉酒,剩下看注释
@k1011Drunk(chara = -1)
	#FUNCTION
	#DIM chara
	SIF chara == -1
		chara = TARGET
	SIF BASE:chara:酒気 <= 2
		RETURNF 0
	SELECTCASE BASE:chara:酒気
		;不省人事
		CASE IS > (MAXBASE:chara:酒気 / 10) * 8
			RETURNF 3
		;酔酒
		CASE IS > (MAXBASE:chara:酒気 / 10) * 5
			RETURNF 2
		;微醺
		CASE IS <= (MAXBASE:chara:酒気 / 10) * 5
			RETURNF 1
	ENDSELECT

