;EXP-------------------------------------------------
	@k1011GetExp, nameOfExp, chara = -999
		#FUNCTION
		#DIMS nameOfExp
		#DIM chara
		SIF chara == -999
			chara = TARGET
		IF K1011Game == "TW"
			SIF nameOfExp == "吸血经验"
				RETURNF k1011GetExpV(chara)
			nameOfExp '= k1011GetTwExpName(nameOfExp)
		ENDIF
		SIF nameOfExp == "--"
			RETURNF 0
		RETURNF EXP:chara:nameOfExp

	@k1011AddExp, nameOfExp, addNum, chara = -999, showInfo = 1
		#DIMS nameOfExp
		#DIM addNum
		#DIM chara
		#DIM showInfo
		SIF chara == -999
			chara = TARGET
		IF K1011Game == "TW"
			IF nameOfExp == "吸血经验"
				CALLF k1011AddExpV(addNum, chara)
				SIF showInfo
					CALL k1011AddExpInfo, nameOfExp, addNum, chara
				RETURN
			ENDIF
			nameOfExp '= k1011GetTwExpName(nameOfExp)
		ENDIF
		SIF nameOfExp == "--"
			RETURN 0
		EXP:chara:nameOfExp += addNum
		SIF showInfo
			CALL k1011AddExpInfo, nameOfExp, addNum, chara

	@k1011SetExp, nameOfExp, num, chara = -999
		#FUNCTION
		#DIMS nameOfExp
		#DIM num
		#DIM chara
		SIF chara == -999
			chara = TARGET
		IF K1011Game == "TW"
			IF nameOfExp == "吸血经验"
				CALLF k1011SetExpV(num, chara)
				RETURNF
			ENDIF
			nameOfExp '= k1011GetTwExpName(nameOfExp)
		ENDIF
		SIF nameOfExp == "--"
			RETURNF 0
		EXP:chara:nameOfExp = num
