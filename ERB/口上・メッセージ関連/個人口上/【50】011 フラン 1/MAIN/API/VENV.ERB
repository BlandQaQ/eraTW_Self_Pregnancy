;吸血鬼----------------------------------------------
	;是否是吸血鬼
	@k1011IsVampirism, chara = -999
		#FUNCTION
		#DIM chara
		SIF chara == -999
			chara = TARGET
		IF K1011Game == "YM"
			RETURNF TALENT:chara:吸血鬼
		ELSEIF K1011Game == "TW"
			RETURNF TALENT:chara:132 && TALENT:chara:191 == 3
		ENDIF

	;变吸血鬼
	@k1011ToVampirism, chara
		#DIM chara
		IF K1011Game == "YM"
			TALENT:chara:吸血鬼 = 1
		ELSEIF K1011Game == "TW"
			TALENT:chara:132 = 1
			TALENT:chara:191 = 3
		ENDIF
