;MARK------------------------------------------------
	@k1011GetMark, nameOfMark, chara
		#FUNCTION
		#DIMS nameOfMark
		#DIM chara
		SIF chara == -999
			chara = TARGET
		SIF K1011Game == "TW"
			nameOfMark '= k1011GetTwMarkName(nameOfMark)
		;没有
		SIF nameOfMark == "--"
			RETURNF 0
		;特殊值
		SELECTCASE nameOfMark
			CASE "反发履历"
				RETURNF MARK:chara:97
			CASE "恐怖履历"
				RETURNF MARK:chara:96
			CASEELSE
				RETURNF MARK:chara:nameOfMark
		ENDSELECT

	@k1011AddMark, nameOfMark, addNum, chara = -999, showInfo = 1
		#DIMS nameOfMark
		#DIM addNum
		#DIM chara
		#DIM showInfo
		SIF chara == -999
			chara = TARGET
		SIF K1011Game == "TW"
			nameOfMark '= k1011GetTwMarkName(nameOfMark)
		;没有
		SIF nameOfMark == "--"
			RETURN 0
		;特殊值
		SELECTCASE nameOfMark
			CASE "反发履历"
				MARK:chara:97 += addNum
			CASE "恐怖履历"
				MARK:chara:96 += addNum
			CASEELSE
				MARK:chara:nameOfMark += addNum
				SIF showInfo
					CALL k1011AddMarkInfo, nameOfMark, addNum, chara
		ENDSELECT

	@k1011SetMark, nameOfMark, num, chara = -999
		#FUNCTION
		#DIMS nameOfMark
		#DIM num
		#DIM chara
		SIF chara == -999
			chara = TARGET
		SIF K1011Game == "TW"
			nameOfMark '= k1011GetTwMarkName(nameOfMark)
		;没有
		SIF nameOfMark == "--"
			RETURNF 0
		;特殊值
		SELECTCASE nameOfMark
			CASE "反发履历"
				MARK:chara:97 = num
			CASE "恐怖履历"
				MARK:chara:96 = num
			CASEELSE
				MARK:chara:nameOfMark = num
		ENDSELECT
;ABL-------------------------------------------------
	@k1011GetAbl, nameOfAbl, chara = -999
		#FUNCTION
		#DIMS nameOfAbl
		#DIM out
		#DIM chara
		SIF chara == -999
			chara = TARGET
		SIF K1011Game == "TW"
			nameOfAbl '= k1011GetTwAblName(nameOfAbl)
		SIF nameOfAbl == "--"
			RETURNF 0
		;ym正常
		IF K1011Game == "YM"
			RETURNF ABL:chara:nameOfAbl
		;tw -> ym
		ELSE
			out = ABL:chara:nameOfAbl
			IF !GROUPMATCH(nameOfAbl, "指技術", "膣技術", "Ａ技術", "胸技術", "舌技術", "腰技術", "清掃技能", "話術技能", "戦闘能力", "教養", "料理技能", "音楽技能")
				SELECTCASE out
					CASE IS >= 50
							out = 5
					CASE IS >= 20
							out = 4
					CASE IS >= 10
							out = 3
					CASE IS >=  3
							out = 2
					CASE IS >=  1
							out = 1
					CASEELSE
							out = 0
				ENDSELECT 
			ENDIF
		ENDIF
		RETURNF MIN(out, 5)

	@k1011AddAbl, nameOfAbl, addNum, chara = -999, showInfo = 1
		#DIMS nameOfAbl
		#DIM addNum
		#DIM chara
		#DIM showInfo
		SIF chara == -999
			chara = TARGET
		SIF K1011Game == "TW"
			nameOfAbl '= k1011GetTwAblName(nameOfAbl)
		SIF nameOfAbl == "--"
			RETURN 0
		ABL:chara:nameOfAbl += addNum
		SIF showInfo
			CALL k1011AddAblInfo, nameOfAbl, addNum, chara

