	@k1011GetNowex, nameOfEx, chara = -999
		#FUNCTION
		#DIMS nameOfEx
		#DIM nowexId
		#DIM chara
		SIF chara == -999
			chara = TARGET
		;寸止特殊处理
		IF GROUPMATCH(nameOfEx, "寸止失败(你先生)", "寸止失败(芙兰)")
			SIF K1011Game == "TW"
				RETURNF 0
			IF nameOfEx == "寸止失败(你先生)"
				RETURNF NOWEX:41
			ELSE
				RETURNF NOWEX:42
			ENDIF
