;数据更新
@updateVersion()
	#DIM index
	;本体更新
	SIF LASTLOAD_VERSION != GAMEBASE_VERSION
		CALL gameUpdateVersion
	;kojo更新
	FOR index, 0, CHARANUM
		CALL KOJO_MESSAGE_SEND("KOJO_VERSION", 0, index)
	NEXT

